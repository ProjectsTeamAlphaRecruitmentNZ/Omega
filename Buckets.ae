<%
Dim systemLogBuckets
                  
Function logBucketChanges
	logBucketChanges=False
	If VarType(systemLogBuckets)=0 Then
		' uninitialized 
		systemLogBuckets=False ' initialize as False, so any errors can just bang out
		
		Dim serverVer : serverVer=169
		Dim clientVer : clientVer=169
		On Error Resume Next
		clientVer=System.clientVersion
		If err Then Exit Function
		
		serverVer=system.serverVersion
		If err Then Exit Function
		If serverVer>=178 and clientVer>=178 Then
			If not system.transactionLog.provider is nothing Then
				' transaction logging is installed - see if bucket logging is enabled
				Dim prop : Set prop=this.Class.Properties("Bucket Logging")
				If not prop is nothing Then
					systemLogBuckets=CBool(prop)
				End If
			End If
		End If
	End If
	logBucketChanges=systemLogBuckets
End Function

Sub LogContentChange(byval transaction)
	If logBucketChanges Then _
		this.logExternalChange "ActivElk.BucketTxLog",this.className & ":" & this.key
End Sub

'//////////////////////////////////////////////////////////////////////////

Dim s_ClassName
Dim s_Key

Sub PreDelete(trans)
	s_ClassName=ClassName
	s_Key=Key
	If logBucketChanges Then _
		this.logExternalChange "ActivElk.BucketTxLog","-" & this.className & ":" & this.key
End Sub

Sub PostDelete
	Set Cls=System.Classes(s_ClassName)
	Cls.DeleteBucket s_Key
End Sub
%>
