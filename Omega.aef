'Calculate Cost of Last week, Total cost to date, Current Balance
Sub EstimatedCost_Init()
	If Form.Object.Folders.Timesheets.Exists("DateDiff('d',WeekEndingDate.Value,DateAdd('d',-(Weekday(Date())+5),Date()),DateAdd('d',1-Weekday(Date()),Date())) = -6") Then
		Dim objTimesheet ' *-- 		Dim objTimesheet as Timesheet
		Set objTimesheet = Form.Object.Folders.Timesheets.Scan("DateDiff('d',WeekEndingDate.Value,DateAdd('d',-(Weekday(Date())+5),Date()),DateAdd('d',1-Weekday(Date()),Date())) = -6",,,"WeekEndingDate Desc").Fetch()
		If Not IsNull(objTimesheet.WeeklyCost) Then
			Form.Controls.Main.EstimatedCost.Controls.lbl_4.text = FormatNumber(objTimesheet.WeeklyCost.Value)
		Else
			Form.Controls.Main.EstimatedCost.Controls.lbl_4.text =0.00
		End If
	End If
	Form.Object.Budget_CostToDate = FormatNumber(Round(form.Object.Folders("Timesheets").Sum("WeeklyCost"),2))
 	Form.Object.Budget_CurrentBalance = FormatNumber(Round(Form.Object.Folders("Budgets").Sum("BudgetAmount"),2)-Round(form.Object.Folders("Timesheets").Sum("WeeklyCost"),2))
End Sub

Function updateActivePO()
	'Initialize Active PO#/ Active RC# from the Budget table
	Dim objBud,objBud1 ' *-- 	Dim objBud,objBud1 as Budget
	If Form.Object.Folders.Budgets.Count>0 Then
		If Form.Object.Folders.Budgets.Exists("DateApplied<=Date() And Date()<=DateExpiry") Then
