Sub ContactCallbtn_Click()
	Set oCommFolder=System.folders.Activities
		Set oCommInst=oCommFolder.CreateNewInstance(System.Classes("stdPhoneCallActivity"))		
		
		Set oDialog = Createobject("Elk9.dialog")
		oDialog.AeSystem=System        
		odialog.saveonok = 1
		oDialog.AeObject=oCommInst 
		oDialog.ContextName="Small"
		oDialog.Title="New Phone Call with " & Form.Object.Contact.Prompt
		oDialog.Buttons=3
'		oDialog.ParentWindow=hwnd
    
		nRet=oDialog.DoModal 
	    
	    If nRet=1 Then
	    	Set oContactInst=oCommInst.Folders("Contacts").CreateNewInstance()
			oContactInst.Contact.value=Form.Object.Contact.value
	    	oContactInst.save
	    	
	    	Set oJobInst=oCommInst.folders("Jobs").CreateNewInstance()
	    	oJobInst.Job=Form.Object
	    	oJobInst.save
	    End If
End Sub

Sub ReportsToCallbtn_Click() 
	Set oCommFolder=System.folders.Activities
		Set oCommInst=oCommFolder.CreateNewInstance(System.Classes("stdPhoneCallActivity"))	
			
		Set oDialog = Createobject("Elk9.dialog")
		oDialog.AeSystem=System        
		odialog.saveonok = 1
		oDialog.AeObject=oCommInst 
		oDialog.ContextName="Small"
		oDialog.Title="New Phone Call with " & Form.Object.ReportsTo.Prompt
		oDialog.Buttons=3
'		dlg.ParentWindow=hwnd
 
		nRet=oDialog.DoModal
		
		If nRet=1 Then
			Set oContactInst=oCommInst.Folders("Contacts").CreateNewInstance()
			oContactInst.Contact.value=Form.Object.ReportsTo.value
	    	oContactInst.save
	    
	   		Set oJobInst=oCommInst.folders("Jobs").CreateNewInstance()
	    	oJobInst.Job=Form.Object
	    	oJobInst.save
	    End If
End Sub

Sub Form_Init() 
	
	If Form.Object.Contact.IsNull Then
		Form.ContactCallbtn.Enabled=False
	Else
		Form.ContactCallbtn.Enabled=True
	End If
	
	If Form.Object.Approver.IsNull Then
		Form.ApproverCallbtn.Enabled=False		
	Else
		Form.ApproverCallbtn.Enabled=True
	End If   
	
	If Form.Object.ReportsTo.IsNull Then
		Form.ReportsToCallbtn.Enabled=False		
	Else
		Form.ReportsToCallbtn.Enabled=True
	End If
	
	Dim TaskCount
	TaskCount=Form.Object.folders("Communications").count("Activity.Complete=FALSE")
	If TaskCount>0 Then
		Form.Controls("Main").Communications.Title = "Communications *"& TaskCount & " Open*"
		Form.Controls("Main").Communications.ForeColor =vbRed
	Else
