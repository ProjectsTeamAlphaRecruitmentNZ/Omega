Sub Form_Init() 
	Dim TaskCount
	TaskCount=Form.object.folders("Communications").count("Activity.IsKindof(""stdTaskActivity"")")
	If TaskCount>0 Then
		Form.Controls("Main").Communications.Title = "Communications *"& TaskCount & " Task(s)*" 
	End If 
End Sub

Sub Username_Choose()  
	form.object.username=LCase(form.object.person.name.firstname) & "." & LCase(form.object.person.name.lastname)
End Sub

Sub ProfileToolBar_Clicked(btnID) 
	Select Case btnID
		Case 1 ' Add
			Dim objArray, iCount,oProfileInst

			' Executes the wizard based on the XML script
			bResult = System.UT_DoScriptWithObjectEx( "omega_scripts.omega_contactprofileselect", "Build Profile", Form.Object, 450, 600)
			     
			' If all goes well
			If bResult Then			     
				' Sets the array containing all of the Profile Value class instances
				objArray = Form.Object.Properties("objArray")                
                                   
				' Adds each Organization instance as a Client of the current 
				' object, saves these and refreshes the grid To display them.
				For iCount = Lbound(objArray) To Ubound(objArray)
					Set oProfileInst=Form.Object.Folders("ContactProfiles").CreateNewInstance()
					oProfileInst.ProfileValue.value= objArray(iCount)
					oProfileInst.ProfileCategory.value= objArray(iCount).ProfileCategory.value
					oProfileInst.save
				Next    
	            Form.Controls.ProfileGrid.Control.Refresh                                              
	        ' If there is a problem creating the Wizard or executing the XML script
	        ' the DoScriptError property contains the error string produced.
	        ' The Cancelled property indicates whether this Wizard was
	        ' just cancelled and so avoids the error handling code.    
	     	ElseIf Not Form.Object.Properties("Cancelled") Then
		     	    MsgBox Form.Object.Properties("DoScriptError")
	     	End If  
	     	
		Case 2 ' Delete
			Set oGrid = Form.Controls.ProfileGrid.Control
			' This is odd, but the SelectedRow is apparently equal to the RowCount, ie. one
			' greater than the highest index when no row is selected, as occurs for instance
			' when the items are sorted by clicking the column header. 
			' DeleteRow may be used if the you wish to delete the entire object instead
			' of just the join.
			If  oGrid.RowCount > 0 And Not oGrid.SelectedRow = oGrid.RowCount Then
				If oGrid.DeleteEnabled Then
					Set selObj = oGrid.RowObject(oGrid.SelectedRow)
					selObj.Delete	
				Else
					MsgBox "Deleting this object is not enabled!", vbOKOnly, "Delete failed!"
				End If
			End If  
			oGrid.Refresh   			
	End Select

End Sub
%>
}

