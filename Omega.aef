	"chkChargedClient" checkbox member "ChargedClient" tag ENU "Charge Client" 327,96,63,9
	"Reason" edit member "Reason" 0,111,393,61 multiline autohscroll toptag
	"txtOriginalQty" edit member "OriginalQty" tag ENU "Calculation Value" 420(50),3,251(100),14 readonly autohscroll sidetag
	"txtCalculationData" edit member "CalculationData" tag ENU "" 420(50),23,253(100),102(50) readonly multiline autovscroll vscroll toptag
	"cmdCalc" button tag ENU "Calculate" 2,201,78,23
		OnClick "cmdCalc_Click"
	"AddedOn" expression ENU "stdRecordStamp.WhenAdd" 399(100),130(50),149(100),14(50) sidetag text ENU "Added On" tagset(3)
	"AddedBy" expression ENU "stdRecordStamp.WhoAdd" 553(100),130(50),120(100),14(50) sidetag text ENU "Added By" tagset(4)
	"UpdatedOn" expression ENU "stdRecordStamp.WhenUpdate" 399(100),148(50),149(100),14(50) sidetag text ENU "UpdatedOn" tagset(3)
	"UpdatedBy" expression ENU "stdRecordStamp.WhoUpdate" 553(100),148(50),120(100),14(50) sidetag text ENU "UpdatedBy" tagset(4)
	"lblAssignmentRate2" text member "AssignmentRate2" tag ENU "(Temp Rate)" 0,66,47,9
	"lblAssignmentRate1" text member "AssignmentRate1" tag ENU "(Client Rate)" 0,89,47,9
	OnPreSave "Form_PreSave"
	Script VBScript
<%
Sub Form_PreSave()
	If Not IsNull(form.Object.Quantity) Or Not IsNull(Form.Object.AssignmentRate2) Then
		form.Object.PaidAmount.Value = form.Object.Quantity * form.Object.AssignmentRate2
	End If
	If Not IsNull(form.Object.Quantity) Or Not IsNull(Form.Object.AssignmentRate1) Then
		form.Object.ChargeAmount.Value = form.Object.Quantity * form.Object.AssignmentRate1
	End If
End Sub
Sub cmdCalc_Click()	
	'1Begin - Check If Job is not NULL
	If Not IsEmpty(Form.Object.Job) Or Not IsNull(Form.Object.Job) Then
		Dim oLeaveInst, oLSdate, oTSInst, arrDay, oPubHoliday
		oLeaveInst = Form.Object
		oLSdate = DateAdd("d",8-Weekday(oLeaveInst.AssignmentStartDate),oLeaveInst.AssignmentStartDate.value)
		arrDay = Array("Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday")
		'2Begin - Check if Timesheet of candidate exists for same date as Leave StartDate
		If oLeaveInst.Candidate.Folders.Timesheets.Count("Job.key =  '"& oLeaveInst.job.key &"' and WeekEndingDate=#" & oLSdate & "# ") > 0 Then
			Set oTSInst = oLeaveInst.Candidate.Folders.Timesheets.Scan("Job.key =  '"& oLeaveInst.job.key &"' and WeekEndingDate=#" & oLSdate & "# ").fetch
			'Assignment Rates
			If oLeaveInst.Candidate.Folders.Timesheets.Count("Job.key =  '"& oLeaveInst.job.key &"' and WeekEndingDate=#" & oLSdate & "# ") > 0 Then
				oLeaveInst.AssignmentRate1 = oTSInst.JobAssignment.AssignmentRate1
				oLeaveInst.AssignmentRate2 = oTSInst.JobAssignment.AssignmentRate2
			End If
			'3Begin - Check If Type is Statutory Leave or DayinLiu
			If (oLeaveInst.LeaveType.value.value = "Stat") Or (oLeaveInst.LeaveType.value.value = "DayInLieu") Then 
				'Check Public Holiday matches with Leave.StartDate
				'Reason
				If System.Folders.PublicHolidays.count("HolidayDate=#"& oLeaveInst.AssignmentStartDate &"# ") > 0 Then
					oPubHoliday = System.Folders.PublicHolidays.Scan("HolidayDate=#"& oLeaveInst.AssignmentStartDate &"# ").Fetch
					oLeaveInst.Reason = oPubHoliday.Holiday.value
				
					For Each oHoliday In system.Folders("PublicHolidays").Scan("HolidayDate=#"& oLeaveInst.AssignmentStartDate &"# ")		
						oHolidaydate = oHoliday.Holidaydate.value
			        	For i=0 To 6
				        	If oLeaveInst.Candidate.Folders.Timesheets.Scan("Job.key =  '"& oLeaveInst.job.key &"' and WeekEndingDate=#" & oLSdate & "# ").fetch.members(arrDay(i)&"Date").Value = oHolidaydate Then
					        	'Quantity, Calculation Value, Calculation Data
								
								Dim totalTS, dailyTotal, dailyCount		       
							    'Calculate Average of last 45 days' timesheets 
							    totalTS = oLeaveInst.Candidate.Folders("Timesheets").Count("Job.Key='"& oLeaveInst.Job.Key &"' and Between(WeekEndingDate,DateAdd('d',-45,'"& oLeaveInst.AssignmentStartDate &"'),DateAdd('d',-1,'"& oLeaveInst.AssignmentStartDate &"')) and "& arrDay(i) &"Total>0")
							    oLeaveInst.CalculationData.value = ""
							    If totalTS>0 Then
							        dailyTotal = 0
							        dailyCount = 0
							        If totalTS > 4 And totalTS <=8 Then
							            lowerLimit = Round(totalTS/2)-2
							            higherLimit = lowerLimit+3
							            oLeaveInst.CalculationData.value = VbCrLf & "===Counted TS Values==="
							            For Each TS In oLeaveInst.Candidate.Folders("Timesheets").scanfrom(lowerLimit,"Job.Key='"& oLeaveInst.Job.Key &"' and Between(WeekEndingDate,DateAdd('d',-45,'"& oLeaveInst.AssignmentStartDate &"'),DateAdd('d',-1,'"& oLeaveInst.AssignmentStartDate &"')) and "& arrDay(i) &"Total>0",,higherLimit,WeekdayName(Weekday(oHolidaydate)) + "Total")
							                For j = 0 To 6
							                    If Weekday(oLeaveInst.Candidate.Folders.Timesheets.Scan("Job.key =  '"& oLeaveInst.job.key &"' and WeekEndingDate=#" & oLSdate & "# ").fetch.members(arrDay(i)&"Date")) = Weekday(TS.members(arrDay(j)&"Date")) Then
							                    	oLeaveInst.CalculationData.value = oLeaveInst.CalculationData.value & VbCrLf & "{" & arrDay(j) & ": " & TS.Members(arrDay(j)&"Date") & "; Hours: " & TS.Members(arrDay(j)&"Total") & "} - JobID: " & oLeaveInst.Job.JobID
							                        dailyTotal = dailyTotal + TS.Members(arrDay(j)&"Total")
							                        dailyCount = dailyCount + 1
							                        Exit For
							                    End If
							                Next
							            Next
							        Else
							        	oLeaveInst.CalculationData.value = VbCrLf & "===Counted TS Values==="
							            For Each TS In oLeaveInst.Candidate.Folders("Timesheets").scan("Job.Key='"& oLeaveInst.Job.Key &"' and Between(WeekEndingDate,DateAdd('d',-45,'"& oLeaveInst.AssignmentStartDate &"'),DateAdd('d',-1,'"& oLeaveInst.AssignmentStartDate &"')) and "& arrDay(i) &"Total>0",,,WeekdayName(Weekday(oHolidaydate)) + "Total")
							                For j = 0 To 6
							                    If Weekday(oLeaveInst.Candidate.Folders.Timesheets.Scan("Job.key =  '"& oLeaveInst.job.key &"' and WeekEndingDate=#" & oLSdate & "# ").fetch.members(arrDay(i)&"Date")) = Weekday(TS.members(arrDay(j)&"Date")) Then
							                    	oLeaveInst.CalculationData.value = oLeaveInst.CalculationData.value & VbCrLf & "{" & arrDay(j) & ": " & TS.Members(arrDay(j)&"Date") & "; Hours: " & TS.Members(arrDay(j)&"Total") & "} - JobID: " & oLeaveInst.Job.JobID
							                        dailyTotal = dailyTotal + TS.Members(arrDay(j)&"Total")
							                        dailyCount = dailyCount + 1
							                        Exit For
							                    End If
							                Next
							            Next
				                    End If   
				                    oLeaveInst.CalculationData.value =  oLeaveInst.CalculationData.value & VbCrLf & VbCrLf & "===Total TS from last 60 days==="         
							        For Each TS In oLeaveInst.Candidate.Folders("Timesheets").scan("Between(WeekEndingDate,DateAdd('d',-60,'"& oLeaveInst.AssignmentStartDate &"'),DateAdd('d',-1,'"& oLeaveInst.AssignmentStartDate &"'))",,,Weekendingdate)
							        	oLeaveInst.CalculationData.value = oLeaveInst.CalculationData.value & VbCrLf & "{" & arrDay(j) & ": " & TS.Members(arrDay(j)&"Date") & "; Hours: " & TS.Members(arrDay(j)&"Total") & "} - JobID: " & oLeaveInst.Job.JobID
							        Next
							        If dailyTotal>0 And dailyCount>0 Then    
							            dailyAverage = dailyTotal/dailyCount
							            result = 0                                                        
							            result = Round(dailyAverage*4,0)/4
							        Else
							            result=0
							        End If
							        oLeaveInst.OriginalQty = result
							        oLeaveInst.Quantity = result
							    Else
							    	If oLeaveInst.Candidate.Folders("Timesheets").count("Between(WeekEndingDate,DateAdd('d',-60,'"& oLeaveInst.AssignmentStartDate &"'),DateAdd('d',-1,'"& oLeaveInst.AssignmentStartDate &"'))")>0 Then
								    	oLeaveInst.CalculationData.value =  oLeaveInst.CalculationData.value & VbCrLf & VbCrLf & "===Total TS from last 60 days==="         
								        For Each TS In oLeaveInst.Candidate.Folders("Timesheets").scan("Between(WeekEndingDate,DateAdd('d',-60,'"& oLeaveInst.AssignmentStartDate &"'),DateAdd('d',-1,'"& oLeaveInst.AssignmentStartDate &"'))",,,WeekEndingDate)
								        	oLeaveInst.CalculationData.value = oLeaveInst.CalculationData.value & VbCrLf & "{" & arrDay(j) & ": " & TS.Members(arrDay(j)&"Date") & "; Hours: " & TS.Members(arrDay(j)&"Total") & "} - JobID: " & oLeaveInst.Job.JobID
								        Next
							    	End If
							        oLeaveInst.OriginalQty = 0
							    End If
					    	End If
			        	Next
		            Next
	            Else
	            	oLeaveInst.OriginalQty = 0
	            	oLeaveInst.Quantity = 0
	            	oLeaveInst.CalculationData.value = ""
	            	oLeaveInst.Reason = ""
				End If
				'Check Public Holiday matches with Leave.StartDate
				
			End If
			'3End - Check If Type is Statutory Leave or DayinLiu
		End If
		'2End - Check if Timesheet of candidate exists for same date as Leave StartDate
		
	End If
	'1End - Check If Job is not NULL		
	
	'Paid Amt & Charge Amt
	If Not IsNull(oLeaveInst.Quantity) Or Not IsNull(oLeaveInst.AssignmentRate2) Then
		form.Object.PaidAmount.Value = form.Object.Quantity * oLeaveInst.AssignmentRate2
	End If
	If Not IsNull(oLeaveInst.Quantity) Or Not IsNull(oLeaveInst.AssignmentRate1) Then
		oLeaveInst.ChargeAmount.Value = oLeaveInst.Quantity * oLeaveInst.AssignmentRate1
	End If
End Sub
%>
}

