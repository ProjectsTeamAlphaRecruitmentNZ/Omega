	Select Case btnID
	Case 1 	'Search 
		FillGrid()		
	End Select
End Sub

Sub FillGrid()  
	ClearGrid()
	Dim strSearch 
	If Form.Object.SearchMinEnd>1 and not Form.object.SearchDivision.IsNull Then
		strSearch=System.FormatString(Form.object.class.properties("BatchSearch"), Array(Form.Object.SearchMinEnd, Form.object.SearchDivision.Key))
		With Form.grdTarget.Control
			.AeFolder=system.folders.JobAssignments   
			.Filter=strSearch
			.fill
			.AllowOpen=False
			.AllowDeletes=False		
		End With   
	ElseIf Form.Object.SearchDivision.IsNull Then 
		MsgBox "A Division must be selected for the search.", 0+48, System.Title
	ElseIf Form.Object.SearchMinEnd>1 Then  
			MsgBox "The Minimum End Date must be greater than 1", 0+48, System.Title
	End If  
End Sub

Sub SearchDivision_Change()
    ClearGrid()
End Sub

Sub SearchMinEnd_Change() 
	ClearGrid() 
End Sub   

Sub ClearGrid()
	Dim SaveChanges
	SaveChanges=NULL                  
	If Form.grdTarget.Control.RowCount>0 Then
		For i=0 To Form.grdTarget.Control.RowCount-1
			If Form.grdTarget.Control.RowObject(i).members("RequiresQC").IsDirty Then
				SaveChanges=MsgBox("Do you want to save your changes?", 4+32, System.Title)
				If SaveChanges=6 Then
					SaveGridChanges(True)
				ElseIf SaveChanges=7 Then
					SaveGridChanges(False)				
				End If
				Exit For	 
			End If 
		Next 	
	End If
    Form.grdTarget.Control.Refresh
End Sub 

Sub SaveGridChanges(blnSave)
    For i=0 To Form.grdTarget.Control.RowCount-1
		If Form.grdTarget.Control.RowObject(i).members("RequiresQC").IsDirty Then
			If blnSave Then
				Form.grdTarget.Control.RowObject(i).Save
			Else
				Form.grdTarget.Control.RowObject(i).Revert				
			End If	 
		End If 
	Next
End Sub
%>
}

