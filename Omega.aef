			If intBudCC1>0 Then
				Set objBudCC1 = Form.Object.Folders.Budgets.scan("Type.value.value='Cost Centre' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudCC1.PONum.Value
			ElseIf intBudRC1>0 Then
				Set objBudRC1 = Form.Object.Folders.Budgets.scan("Type.value.value='RC Code' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudRC1.PONum.Value
			ElseIf intBudSID1>0 Then
				Set objBudSID1 = Form.Object.Folders.Budgets.scan("Type.value.value='Supplier ID' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudSID1.PONum.Value
			ElseIf intBudDID1>0 Then
				Set objBudDID1 = Form.Object.Folders.Budgets.scan("Type.value.value='Department ID' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudDID1.PONum.Value
			Else
				Form.Object.JobPONum2.Value = ""
			End If
		Else
			Form.Object.JobPONum.Value = ""
			Form.Object.JobPONum2.Value = ""
		End If
		
	Else
		Form.Object.JobPONum.Value = ""
		Form.Object.JobPONum2.Value = ""
	End If
	If Form.Object.Folders.Budgets.Count=0 Then	
		form.Object.JobPONum.Value = ""
		form.Object.JobPONum2.Value=""
	End If
End Function

Sub Form_PreSave()	
	'Add Job Profile - Service Job Family = Payroll If Fees_PayrollOnly is SET to TRUE
	currentvalue = "Payroll"	
	If Not form.Object.Fees_PayrollOnly.Value = True Then
		If form.Object.folders("JobProfiles").exists("ProfileValue.Profileval = '" & currentvalue & "'") Then
			For Each jobprofilevalue In form.Object.folders("JobProfiles").Scan("ProfileValue.Profileval = '" & currentvalue & "'")
        		valuetodelete=jobprofilevalue.key
	            Set deletejobprofilevalue = system.classes("JobProfile").getinstance(valuetodelete)
	            deletejobprofilevalue.delete
			Next
		End If
	Else
	    If Not form.Object.folders("JobProfiles").exists("ProfileValue.Profileval = '" & currentvalue & "'") Then
	    	For Each jobprofilevalue In system.folders("prof_profilevalue").Scan("Profileval = '" & currentvalue & "'")
		        Set newprofilevalue = System.classes("prof_profilevalue").getinstance(jobprofilevalue.Key)
		        Set newprofilecategory = System.classes("prof_profilecategory").getinstance(jobprofilevalue.profilecategory.key)
		        Set newjobprofilevalue=form.Object.folders("JobProfiles").createnewinstance()
		        newjobprofilevalue.profilevalue.value=newprofilevalue
		        newjobprofilevalue.profilecategory.value=newprofilecategory
		        newjobprofilevalue.save
	    	Next
	    End If		
	End If
	Call updateActivePO()
End Sub

Sub grdBudget_OnChange()
	Call updateActivePO()
End Sub

Sub Details_Init()
	'Initialize Client & Contractor Rate
	Form.Object.JobRate1.Value = Form.Object.TotalFees.Value
	Call updateActivePO()
End Sub

Sub Total_Click()
	Call DefineValue()
	BaseRate = Form.Object.JobRate2.Value	
	If Not(Form.Object.JobRate2.Value<1) Then
		If Not IsNull(Form.Object.JobRate2) And Form.Object.Fees_Provider.Value=0.00 Then		
			Provider = (Form.Object.Fees_ProviderAmount * 100)/BaseRate
			Form.Object.Fees_Provider.Value=Round(Provider,2)
			Provider = Form.Object.Fees_ProviderAmount
		Else
			Provider = (Form.Object.Fees_Provider * BaseRate)/100
			Form.Object.Fees_ProviderAmount.Value=Round(Provider,2)
		End If
		
		If Not IsNull(Form.Object.JobRate2) And Form.Object.Fees_Admin.Value=0.00 Then
			Admin = (Form.Object.Fees_AdminAmount * 100)/( Provider  + BaseRate )	
			Form.Object.Fees_Admin.Value=Round(Admin,2)	
			Admin = Form.Object.Fees_AdminAmount.Value
		Else
			Admin = (( Provider  + BaseRate ) * Form.Object.Fees_Admin)/100	
			Form.Object.Fees_AdminAmount.Value=Round(Admin,2)
		End If
		
		If Not Form.Object.Fees_PIFPayType = True Or IsNull(Form.Object.Fees_PIFPayType) Then
			Form.Object.TotalFees = Round(BaseRate + Provider + Admin,2)
			Form.Object.TotalAmount=Round(Provider + Admin,2)
		Else
			Form.Object.TotalFees = Round(BaseRate + Provider + Admin + Form.Object.Fees_PIFAmount,2)
			Form.Object.TotalAmount=Round(Provider + Admin + Form.Object.Fees_PIFAmount,2)
		End If
	End If
End Sub

Sub cmdBudget_EstimatedAmount_Click()
	form.Object.Budget_EstimatedAmount = form.Object.TotalFees*form.Object.Budget_EstimatedJobDuration
End Sub
%>
