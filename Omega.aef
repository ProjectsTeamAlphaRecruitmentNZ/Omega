<%
Sub FillDocument()
	' if the document is embedded, and not dirty then load to a temp file then 
	' display the image on the screen
'	On Error Resume Next
	
	If Not this.Object.Person.Photo.IsNull Then
		If Not IsNull(this.Object.Person.Photo.Content) Then
			If this.Object.Person.Photo.mimetype="image/jpeg" Or	this.Object.Person.Photo.mimetype="image/bmp" Or this.Object.Person.Photo.mimetype="image/png" Then
				form.cntlPhoto.control.LoadFromFile this.Object.Person.Photo.extract()
				form.txtNoPhoto.Visible=False
				form.cntlPhoto.Visible=True
			Else
			Form.txtNoPhoto.Text="Photo format not supported"
			form.txtNoPhoto.Visible=True
			form.cntlPhoto.Visible=False
			End If
		Else
		Form.txtNoPhoto.Text="No photo loaded"
			form.txtNoPhoto.Visible=True
			form.cntlPhoto.Visible=False
		End If 
  	Else
  			Form.txtNoPhoto.Text="No photo loaded"
			form.txtNoPhoto.Visible=True
			form.cntlPhoto.Visible=False
	End If	
'	On Error Goto 0
End Sub

Sub Form_Init()
	FillDocument
	form.cntlPhoto.Enabled=False
End Sub
%>
}

