Function LoadBillingInfo()
	'Load Billing Info
	If Not Form.Object.IsKindOf("PermJob") Then
		'Form.Object.MYOB_FeeStructure.Value="Standard"
		'Form.Object.MYOB_Salesline.Value="Week"
		
		If Not Form.Object.Department.IsNull Then
			If Not IsNull(Form.Object.Department.DepartmentID) Then
				Form.Object.JobPONum2.Value = form.Object.Department.DepartmentID.Value
			End If
			If Not Form.Object.Department.Organisation.Accounts.IsNull Then
				Form.Object.MYOB_Inv_Email1.Value = Form.Object.Department.Organisation.Accounts.eMailAddress.eMailAddress.Value
			End If
			If Not Form.Object.Contact.IsNull Then
				Form.Object.MYOB_AttentionTo.Value = Form.Object.Contact.Value
			Else
				Form.Object.MYOB_AttentionTo.Value = ""
			End If
			'If Not Form.Object.Consultant.IsNull Then
				'If form.Object.Consultant.Division.Code.Value="CMSA" Then
					'form.Object.MYOB_FeeStructure.Value="Payroll"
				'End If
			'End If
			
			If Form.Object.Department.Organisation.Folders.OrganisationProfiles.Exists("ProfileCategory.CatCode='AoGS' And ProfileValue.ProfileVal='Participating'") Then
				form.Object.MYOB_FeeStructure.Value = "Payroll"
			End If
			If Not Form.Object.MYOB_AttentionTo.IsNull Then
				Form.Object.MYOB_Inv_Email2.Value = Form.Object.MYOB_AttentionTo.eMailAddress.eMailAddress.Value
				form.Object.MYOB_PurchaseOrg.Value = Form.Object.MYOB_AttentionTo.PrimeDepartment.Department.Organisation.MYOB_Name
				form.Object.MYOB_PurchaseDept.Value = Form.Object.MYOB_AttentionTo.PrimeDepartment.Department.Name
				Form.Object.MYOB_BillingAddress.Address.Value = Form.Object.MYOB_AttentionTo.PrimeDepartment.Department.MailAddress.Address.Value
				Form.Object.MYOB_BillingAddress.City.Value = Form.Object.MYOB_AttentionTo.PrimeDepartment.Department.MailAddress.City.Value
				Form.Object.MYOB_BillingAddress.Suburb.Value = Form.Object.MYOB_AttentionTo.PrimeDepartment.Department.MailAddress.Suburb.Value
				Form.Object.MYOB_BillingAddress.PostCode.Value = Form.Object.MYOB_AttentionTo.PrimeDepartment.Department.MailAddress.PostCode.Value
				Form.Object.MYOB_BillingAddress.Country.Value = Form.Object.MYOB_AttentionTo.PrimeDepartment.Department.MailAddress.Country.Value
			Else
				Form.Object.MYOB_Inv_Email2.Value = ""
				Form.Object.MYOB_PurchaseDept.Value = ""
				Form.Object.MYOB_BillingAddress.Address.Value = ""
				Form.Object.MYOB_BillingAddress.City.Value = ""
				Form.Object.MYOB_BillingAddress.Suburb.Value = ""
				Form.Object.MYOB_BillingAddress.PostCode.Value = ""
			End If
