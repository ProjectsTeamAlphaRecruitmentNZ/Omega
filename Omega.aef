		End If		
		If Not IsNull(Form.Object.JobRate3) And Form.Object.Fees_Admin.Value=0.00 Then
			Admin = (Form.Controls.Main.EstimatedCost.Fees_AdminAmount * 100)/( Provider  + BaseRate )	
			Form.Object.Fees_Admin.Value=Round(Admin,2)		
			Admin = Form.Object.Fees_AdminAmount.Value
		Else
			Admin = (( Provider  + BaseRate ) * Form.Object.Fees_Admin)/100	
			Form.Controls.Main.EstimatedCost.Fees_AdminAmount.Value=Round(Admin,2)
