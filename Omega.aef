Sub ProfileToolBar_Clicked(btnID) 
	Select Case btnID
		Case 1 ' Add
			Dim objArray, iCount,oProfileInst
			' Executes the wizard based on the XML script
			bResult = System.UT_DoScriptWithObjectEx( "omega_scripts.omega_orgprofileselect", "Build Profile", Form.Object, 450, 600)
			' If all goes well
			If bResult Then			     
				' Sets the array containing all of the Profile Value class instances
				objArray = Form.Object.Properties("objArray")        
                                   
				' Adds each Organization instance as a Client of the current 
				' object, saves these and refreshes the grid To display them.
				For iCount = LBound(objArray) To UBound(objArray)
					Set oProfileInst=Form.Object.Folders("OrganisationProfiles").CreateNewInstance()
					oProfileInst.ProfileValue.value= objArray(iCount)
					oProfileInst.ProfileCategory.value= objArray(iCount).ProfileCategory.value
					oProfileInst.save
				Next    
	            Form.Controls.ProfileGrid.Control.Refresh                                              
	        ' If there is a problem creating the Wizard or executing the XML script
	        ' the DoScriptError property contains the error string produced.
	        ' The Cancelled property indicates whether this Wizard was
	        ' just cancelled and so avoids the error handling code.    
	     	Elseif Not Form.Object.Properties("Cancelled") Then
		     	    MsgBox Form.Object.Properties("DoScriptError")
	     	End If  
	     	
		Case 2 ' Delete
			Set oGrid = Form.Controls.ProfileGrid.Control
			' This is odd, but the SelectedRow is apparently equal to the RowCount, ie. one
			' greater than the highest index when no row is selected, as occurs for instance
			' when the items are sorted by clicking the column header. 
			' DeleteRow may be used if the you wish to delete the entire object instead
			' of just the join.
			If  oGrid.RowCount > 0 And Not oGrid.SelectedRow = oGrid.RowCount Then
				If oGrid.DeleteEnabled Then
					Set selObj = oGrid.RowObject(oGrid.SelectedRow)
					selObj.Delete	
				Else
					MsgBox "Deleting this object is not enabled!", vbOKOnly, "Delete failed!"
				End If
			End If  
			oGrid.Refresh   			
	End Select
End Sub

Sub Form_Init()
	ass = system.folders.tempassignments.count("Job.Department.Organisation.key= '"& this.Object.Key &"'")
	eng = system.folders.ContractEngagements.count("Job.Department.Organisation.key= '"& this.Object.Key &"'")
	plc = system.folders.PermPlacements.count("Job.Department.Organisation.key= '"& this.Object.Key &"'")
	cds = System.Folders.Candidate.Count("TempAssignments.Job.Department.Organisation.key= '"& this.Object.Key &"'") + System.Folders.Candidate.Count("PermPlacements.Job.Department.Organisation.key= '"& this.Object.Key &"'") + System.Folders.Candidate.Count("ContractEngagements.Job.Department.Organisation.key= '"& this.Object.Key &"'")
	hsI = System.Folders.HS_Incident.Count("Department.Organisation.key= '"& this.Object.Key &"'") + System.Folders.HS_Event.Count("Department.Organisation.key= '"& this.Object.Key &"'")
	dpr = 0
	If System.Folders.Jobs.Count("Department.Organisation.key= '"& this.Object.Key &"' and Budget_CostToDate>0") > 0 Then
		For Each job In System.Folders.Jobs.Scan("Department.Organisation.key= '"& this.Object.Key &"' and Budget_CostToDate>0")
			'If Not IsNull(job.Budget_CostToDate) Then
				sumBudgetsCosttoDate = 0
				sumBudgetsCosttoDate = sumBudgetsCosttoDate + job.Budget_CostToDate
			'End If
			dpr = CLng(FormatNumber(Round(sumBudgetsCosttoDate,2)))
		Next
	End If
	this.expAssignments.Expression = ass
	this.expEngagements.Expression = eng
	this.expPlacements.Expression = plc
	this.expCandidateSub.Expression = cds
	this.expHSIncidents.Expression = hsI
	this.expDeptRevenue.Expression = dpr
End Sub
%>
}

