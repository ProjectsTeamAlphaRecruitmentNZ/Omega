		Form.Object.JobRate3.Value=0.00
	End If
	If IsNull(Form.Object.Fees_Provider) Then
		Form.Object.Fees_Provider.Value=0.00
	End If	
	If IsNull(Form.Object.Fees_Admin) Then
		Form.Object.Fees_Admin.Value=0.00
	End If
	If IsNull(Form.Object.Fees_ProviderAmount) Then
		Form.Object.Fees_ProviderAmount.Value = 0.00
	End If
	If IsNull(Form.Object.Fees_AdminAmount) Then
		Form.Object.Fees_AdminAmount.Value = 0.00
	End If		
	If IsNull(Form.Controls.Main.EstimatedCost.Fees_ProviderAmount) Or Form.Controls.Main.EstimatedCost.Fees_ProviderAmount.Value="" Then
		Form.Controls.Main.EstimatedCost.Fees_ProviderAmount.Value = 0.00
	End If
	If IsNull(Form.Controls.Main.EstimatedCost.Fees_AdminAmount) Or Form.Controls.Main.EstimatedCost.Fees_AdminAmount.Value="" Then
		Form.Controls.Main.EstimatedCost.Fees_AdminAmount.Value = 0.00
	End If	
End Sub

Sub cmdTotal_Click()
	Call DefineValue()
	BaseRate = Form.Object.JobRate2.Value
	If Not(Form.Object.JobRate2.Value<1) Then
		If Not IsNull(Form.Object.JobRate2) And Form.Object.Fees_Provider.Value=0.00 Then		
			Provider = (Form.Object.Fees_ProviderAmount * 100)/BaseRate
			Form.Object.Fees_Provider.Value=Round(Provider,2)
			Provider = Form.Object.Fees_ProviderAmount
		Else
			Provider = (Form.Object.Fees_Provider * BaseRate)/100
			Form.Object.Fees_ProviderAmount.Value=Round(Provider,2)
		End If
		
		If Not IsNull(Form.Object.JobRate2) And Form.Object.Fees_Admin.Value=0.00 Then
			Admin = (Form.Object.Fees_AdminAmount * 100)/( Provider  + BaseRate )	
			Form.Object.Fees_Admin.Value=Round(Admin,2)	
			Admin = Form.Object.Fees_AdminAmount.Value
		Else
			Admin = (( Provider  + BaseRate ) * Form.Object.Fees_Admin)/100	
			Form.Object.Fees_AdminAmount.Value=Round(Admin,2)
		End If		
		Form.Object.TotalFees = Round(BaseRate + Provider + Admin,2)
		Form.Object.TotalAmount=Round(Provider + Admin,2)
	End If
End Sub

Sub cmdTotal2_Click()
	Call DefineValue()	
	BaseRate = Form.Object.JobRate3.Value
	If Not(Form.Object.JobRate3.Value<1) Then
		If Not IsNull(Form.Object.JobRate3) And Form.Object.Fees_Provider.Value=0.00 Then		
			Provider = (Form.Controls.Main.EstimatedCost.Fees_ProviderAmount * 100)/BaseRate
