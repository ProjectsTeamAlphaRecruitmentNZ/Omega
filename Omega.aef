Sub EstimatedCost_Init()
	If Form.Object.Folders.Timesheets.Exists("DateDiff('d',WeekEndingDate.Value,DateAdd('d',-(Weekday(Date())+5),Date()),DateAdd('d',1-Weekday(Date()),Date())) = -6") Then
		Dim objTimesheet ' *-- 		Dim objTimesheet as Timesheet
		Set objTimesheet = Form.Object.Folders.Timesheets.Scan("DateDiff('d',WeekEndingDate.Value,DateAdd('d',-(Weekday(Date())+5),Date()),DateAdd('d',1-Weekday(Date()),Date())) = -6",,,"WeekEndingDate Desc").Fetch()
		If Not IsNull(objTimesheet.WeeklyCost) Then
			Form.Controls.Main.EstimatedCost.Controls.lbl_4.text = FormatNumber(objTimesheet.WeeklyCost.Value)
		Else
			Form.Controls.Main.EstimatedCost.Controls.lbl_4.Text =0.00
		End If
	End If
	Form.Object.Budget_CostToDate = FormatNumber(Round(form.Object.Folders("Timesheets").Sum("WeeklyCost"),2))
 	Form.Object.Budget_CurrentBalance = FormatNumber(Round(Form.Object.Folders("Budgets").Sum("BudgetAmount"),2)-Round(form.Object.Folders("Timesheets").Sum("WeeklyCost"),2))
End Sub

Function updateActivePO()
	'Initialize Active PO#/ Active RC# from the Budget table
	Dim objBud,objBud1 ' *-- 	Dim objBud,objBud1 as Budget
	If Form.Object.Folders.Budgets.Count>0 Then
		If Form.Object.Folders.Budgets.Exists("DateApplied<=Date() And Date()<=DateExpiry") Then
			intBudPO = Form.Object.Folders.Budgets.Count("Type.value.value='PO Number' And DateApplied<=Date() And Date()<=DateExpiry")
			If intBudPO>0 Then
				Set objBudPO =  Form.Object.Folders.Budgets.scan("Type.value.value='PO Number' And DateApplied<=Date() And Date()<=DateExpiry",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum.Value = objBudPO.PONum.Value
			Else
				Form.Object.JobPONum.Value =""
			End If
			
			intBudCC = Form.Object.Folders.Budgets.Count("Type.value.value='Cost Centre' And DateApplied<=Date() And Date()<=DateExpiry")
			intBudRC = Form.Object.Folders.Budgets.Count("Type.value.value='RC Code' And DateApplied<=Date() And Date()<=DateExpiry")
			intBudSID = Form.Object.Folders.Budgets.Count("Type.value.value='Supplier ID' And DateApplied<=Date() And Date()<=DateExpiry")
			intBudDID = Form.Object.Folders.Budgets.Count("Type.value.value='Department ID' And DateApplied<=Date() And Date()<=DateExpiry")
			If intBudCC>0 Then
				Set objBudCC = Form.Object.Folders.Budgets.scan("Type.value.value='Cost Centre' And DateApplied<=Date() And Date()<=DateExpiry",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudCC.PONum.Value
			ElseIf intBudRC>0 Then
				Set objBudRC = Form.Object.Folders.Budgets.scan("Type.value.value='RC Code' And DateApplied<=Date() And Date()<=DateExpiry",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudRC.PONum.Value
			ElseIf intBudSID>0 Then
				Set objBudSID = Form.Object.Folders.Budgets.scan("Type.value.value='Supplier ID' And DateApplied<=Date() And Date()<=DateExpiry",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudSID.PONum.Value
			ElseIf intBudDID>0 Then
				Set objBudDID = Form.Object.Folders.Budgets.scan("Type.value.value='Department ID' And DateApplied<=Date() And Date()<=DateExpiry",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudDID.PONum.Value
			Else
				Form.Object.JobPONum2.Value = ""
			End If	
			
		ElseIf Form.Object.Folders.Budgets.Exists("DateExpiry<Date()") Then
			intBudPO1 = Form.Object.Folders.Budgets.Count("Type.value.value='PO Number' And DateExpiry<Date()")
			If intBudPO1>0 Then 
				Set objBudPO1 = Form.Object.Folders.Budgets.Scan("Type.value.value='PO Number' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum.Value = objBudPO1.PONum.Value
			Else
				Form.Object.JobPONum.Value =""
			End If
			
			intBudCC1 = Form.Object.Folders.Budgets.Count("Type.value.value='Cost Centre' And DateExpiry<Date()")
			intBudRC1 = Form.Object.Folders.Budgets.Count("Type.value.value='RC Code' And DateExpiry<Date()")
			intBudSID1 = Form.Object.Folders.Budgets.Count("Type.value.value='Supplier ID' And DateExpiry<Date()")
			intBudDID1 = Form.Object.Folders.Budgets.Count("Type.value.value='Department ID' And DateExpiry<Date()")
			If intBudCC1>0 Then
				Set objBudCC1 = Form.Object.Folders.Budgets.scan("Type.value.value='Cost Centre' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudCC1.PONum.Value
			ElseIf intBudRC1>0 Then
				Set objBudRC1 = Form.Object.Folders.Budgets.scan("Type.value.value='RC Code' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudRC1.PONum.Value
			ElseIf intBudSID1>0 Then
				Set objBudSID1 = Form.Object.Folders.Budgets.scan("Type.value.value='Supplier ID' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudSID1.PONum.Value
			ElseIf intBudDID1>0 Then
				Set objBudDID1 = Form.Object.Folders.Budgets.scan("Type.value.value='Department ID' And DateExpiry<Date()",,,"DateExpiry Desc").fetch()
				Form.Object.JobPONum2.Value = objBudDID1.PONum.Value
			Else
				Form.Object.JobPONum2.Value = ""
			End If
		Else
			Form.Object.JobPONum.Value = ""
			Form.Object.JobPONum2.Value = ""
		End If
		
	Else
		Form.Object.JobPONum.Value = ""
		Form.Object.JobPONum2.Value = ""
	End If
	If Form.Object.Folders.Budgets.Count=0 Then	
		form.Object.JobPONum.Value = ""
		form.Object.JobPONum2.Value=""
	End If
End Function

Sub Form_PreSave()	
	'Add Job Profile - Service Job Family = Payroll If Fees_PayrollOnly is SET to TRUE
	currentvalue = "Payroll"	
	If Not form.Object.Fees_PayrollOnly.Value = True Then
		If form.Object.folders("JobProfiles").exists("ProfileValue.Profileval = '" & currentvalue & "'") Then
			For Each jobprofilevalue In form.Object.folders("JobProfiles").Scan("ProfileValue.Profileval = '" & currentvalue & "'")
        		valuetodelete=jobprofilevalue.key
	            Set deletejobprofilevalue = system.classes("JobProfile").getinstance(valuetodelete)
	            deletejobprofilevalue.delete
			Next
		End If
	Else
	    If Not form.Object.folders("JobProfiles").exists("ProfileValue.Profileval = '" & currentvalue & "'") Then
	    	For Each jobprofilevalue In system.folders("prof_profilevalue").Scan("Profileval = '" & currentvalue & "'")
		        Set newprofilevalue = System.classes("prof_profilevalue").getinstance(jobprofilevalue.Key)
		        Set newprofilecategory = System.classes("prof_profilecategory").getinstance(jobprofilevalue.profilecategory.key)
		        Set newjobprofilevalue=form.Object.folders("JobProfiles").createnewinstance()
		        newjobprofilevalue.profilevalue.value=newprofilevalue
		        newjobprofilevalue.profilecategory.value=newprofilecategory
		        newjobprofilevalue.save
	    	Next
	    End If		
	End If	
	Call updateActivePO()
End Sub

