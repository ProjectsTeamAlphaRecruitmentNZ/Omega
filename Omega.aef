Sub On_CreateWeeklyCalls2(Control)
'--oAssignmentInst Function generates the Weekly Calls for a given users Temporary Jobs

Dim iTaskCount, i,StartDate, EndDate,CurrentContact,oContactCall,oCandidateCall, CurrentJob  
Dim oProgressUI: Set oProgressUI = Nothing
Dim bCancelled

'Get the Dates of the Start of oAssignmentInst week (Monday) and the End of oAssignmentInst week (Sunday)
StartDate=DateAdd("d",-(Weekday(Date())-2),Date())
EndDate=DateAdd("d",8-Weekday(Date()),Date()) 

Set oAssignmentFolder=system.Folders.JobAssignments

On Error Resume Next
Set oProgressUI = CreateObject("ActivElk.ProgressDlg")
On Error GoTo 0

'--Get the Assignments that need weekly calls that are owned by the current user
Set oWeeklyCallScan=oAssignmentFolder.Scan("NextCallDate>=DateAdd(""d"",-(Weekday(Date())-2),Date()) and NextCallDate<=DateAdd(""d"",8-Weekday(Date()),Date()) and Job.Consultant=User.Object",,,"Job.ReportsTo; Job")

'Get a list of the old Weekly calls assigned to this user
Set oOldTaskFolder=system.Folders.Tasks.scan("AssignedTo=User.Object and Category=""Weekly"" and Due.Start>=DateAdd(""d"",-(Weekday(Date())+5),Date()) and Due.Start<=DateAdd(""d"",1-Weekday(Date()),Date())")
iCount =system.Folders.Tasks.count("AssignedTo=User.Object and Category=""Weekly"" and Due.Start>=DateAdd(""d"",-(Weekday(Date())+5),Date()) and Due.Start<=DateAdd(""d"",1-Weekday(Date()),Date())")

' **********Testing Only**************
'Set oWeeklyCallScan=oAssignmentFolder.Scan("Job.Consultant=User.Object",,,"Job.ReportsTo;Job")

'Set oOldTaskFolder=system.Folders.Tasks.scan("AssignedTo=User.Object and Category=""Weekly""")
'iCount =system.Folders.Tasks.count("AssignedTo=User.Object and Category=""Weekly""")
'************Testing Only**************   

If Not oProgressUI Is Nothing Then
	oProgressUI.Visible = True
	'iCount parameter is used to control the progress bar, the value in iCount is the maximum value at which the progress bar reaches completion.
	oProgressUI.Initialize iCount, "Deleting Your Old Weekly Calls"
End If

i=0
For Each oTaskInst in oOldTaskFolder
	If Not oProgressUI Is Nothing Then
		'Give the user the chance to push the cancel button while the script is running
		If oProgressUI.Cancelled Then
			bCancelled = True
			Exit For
		End If
	End If 
	
	oTaskInst.delete
	
	i=i+1  
	
	If Not oProgressUI Is Nothing Then
		'send a message to the user "scanning " and update the progress bar with i as a percentage of iCount
		oProgressUI.Update "Deleting...", i
	End If
Next

iCount = oAssignmentFolder.count("NextCallDate>=DateAdd(""d"",-(Weekday(Date())-2),Date()) and NextCallDate<=DateAdd(""d"",8-Weekday(Date()),Date()) and Job.Consultant=User.Object")

'*********Testing Only***************
'iCount = oAssignmentFolder.count("Job.Consultant=User.Object")
'*********Testing Only******************

If Not oProgressUI Is Nothing Then
	oProgressUI.Visible = True
	'iCount parameter is used to control the progress bar, the value in iCount is the maximum value at which the progress bar reaches completion.
	oProgressUI.Initialize iCount, "Creating Your Weekly Calls"
End If

iTaskCount=0
i=0
bCancelled=False
For Each oAssignInst in oWeeklyCallScan

	If Not oProgressUI Is Nothing Then
		'Give the user the chance to push the cancel button while the script is running
		If oProgressUI.Cancelled Then
			bCancelled = True
			Exit For
		End If
	End If
	
	If Not oProgressUI Is Nothing Then
		'send a message to the user "scanning " and update the progress bar with i as a percentage of iCount
		oProgressUI.Update "Creating...", i
	End If

	If not IsNull(oAssignInst.CallFrequency.value) or oAssignInst.CallFrequency.value=0 Then 
	
		Dim CandidateNotes, ContactNotes

		'Build Notes for Task 
	    If not oAssignInst.Job.Department.Isnull Then
		   	CandidateNotes="At " & oAssignInst.Job.Department.prompt & "/" & oAssignInst.ContactablePhone.Number
   			If oAssignInst.ContactablePhone.Extn>"" Then CandidateNotes=CandidateNotes & " x" & oAssignInst.ContactablePhone.Extn
   		End If
   	   
   	    If not oAssignInst.Job.ReportsTo.IsNull Then
		   	ContactNotes="For " & oAssignInst.Candidate.Name.FirstName & " "& oAssignInst.Candidate.Name.LastName & "/" & oAssignInst.Job.ReportsTo.DDI.prompt  
			If oAssignInst.Job.ReportsTo.DDI.Extn>"" Then ContactNotes=ContactNotes& " x" & oAssignInst.Job.ReportsTo.DDI.Extn
		End If
		
		'Create the Weekly Call for the Reports To and then link the other jobs to it 
		'that need a weekly call as well
		If not oAssignInst.Job.ReportsTo.IsNull Then
			If CurrentContact<>oAssignInst.Job.ReportsTo.key Then
				CurrentContact=oAssignInst.Job.ReportsTo.key
				Set oContactCall=system.CreateAssignmentTask ("Weekly", oAssignInst.NextCallDate,"",  oAssignInst.Job.Consultant)
				iTaskCount=iTaskCount+1
				system.CreateAttendee oContactCall, "Contacts", "Contact", oAssignInst.Job.ReportsTo
			End If
			If CurrentJob<>oAssignInst.Job.key Then
				CurrentJob=oAssignInst.Job.key
				system.CreateJobAttendee oContactCall, oAssignInst.Job, nothing
			End If
			If not IsNull(oContactCall.Notes) Then
				oContactCall.Notes=oContactCall.Notes & ", " & Right(ContactNotes, (Len(ContactNotes)-4))
			Else
				oContactCall.Notes=ContactNotes				
			End If
			oContactCall.save
		End If				
		
		'Create the Weekly Call for the temp
		Set oCandidateCall=system.CreateAssignmentTask ("Weekly", oAssignInst.NextCallDate, CandidateNotes, oAssignInst.Job.Consultant) 
		system.CreateAttendee oCandidateCall, "Candidates", "Candidate",  oAssignInst.Candidate
		system.CreateJobAttendee oCandidateCall, oAssignInst.Job, oAssignInst
		iTaskCount=iTaskCount+1
	    i=i+1
	    
		'To see if oAssignmentInst Assignment qualifies for another weekly call, check the end dates
		'against the frequency (+ 5 days)
		If oAssignInst.AssignmentEndDate>=DateAdd("d",(oAssignInst.CallFrequency.value+5),oAssignInst.NextCallDate) Then
			oAssignInst.NextCallDate=DateAdd("d",oAssignInst.CallFrequency.value,oAssignInst.NextCallDate)
		    oAssignInst.save  
		Else
			oAssignInst.NextCallDate=""
			oAssignInst.save
		End If 
	End If
Next

	'clean up the dialog when finished
	If Not oProgressUI Is Nothing Then
		oProgressUI.Visible = False
		Set oProgressUI = Nothing
	End If

	'--Tell the user how many were created
    ok=MsgBox(iTaskCount & " Weekly Calls created between " & FormatDateTime(StartDate,1) & " and " & FormatDateTime(EndDate,1),0, "Weekly Calls")
End Sub  





Sub On_CreateWeeklyCalls(Control) 

Dim objSchedule

Set objSchedule = System.Folders.WeeklyCallSchedule.CreateNewInstance()
 
	
		Dim objPopUp, CallCount
		Set objPopUp = CreateObject("Elk9.Dialog")
		With objPopUp
			.AeSystem = System
			.AeObject = objSchedule
			.FormName = "WeeklyCallSchedule"
			.Title = "Create Weekly Calls"
			.Buttons = 3
		End With
		If objPopUp.DoModal()= 1 Then
			' OK, set status
			CallCount=System.ProcessWeeklyCalls (objSchedule.StartDate, objSchedule.EndDate, objSchedule.User.Key, objSchedule.DeleteOldCalls)
			ok=MsgBox(CallCount & " Weekly Calls created between " & FormatDateTime(objSchedule.StartDate,1) & " and " & FormatDateTime(objSchedule.EndDate,1) & " for " & objSchedule.User.Prompt,0, "Weekly Calls")
			
		End If	

	
End Sub
%>
}

