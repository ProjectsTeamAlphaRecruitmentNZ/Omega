	Script VBScript
<%
Sub DefineValue()	
	If IsNull(Form.Object.AssignmentRate1) Then
		Form.Object.AssignmentRate1.Value=0.00
	End If
	If IsNull(Form.Object.AssignmentRate2) Then
		Form.Object.AssignmentRate2.Value=0.00
	End If
	If IsNull(Form.Object.Fees_Provider) Then
		Form.Object.Fees_Provider.Value=0.00
	End If	
	If IsNull(Form.Object.Fees_Admin) Then
		Form.Object.Fees_Admin.Value=0.00
	End If
	If IsNull(Form.Object.Fees_ProviderAmount) Then
		Form.Object.Fees_ProviderAmount.Value = 0.00
	End If
	If IsNull(Form.Object.Fees_AdminAmount) Then
		Form.Object.Fees_AdminAmount.Value = 0.00
	End If
	If IsNull(Form.Object.Fees_OnCost) Then
		Form.Object.Fees_OnCost.Value=0.00
	End If	
	If IsNull(Form.Object.Fees_OnCostAmount) Then
		Form.Object.Fees_OnCostAmount.Value=0.00
	End If	
	If IsNull(Form.Object.TotalFees) Then
		Form.Object.TotalFees.Value=0.00
	End If
	If IsNull(Form.Object.TotalAmount) Then
		Form.Object.TotalAmount.Value=0.00
	End If
	If IsNull(Form.Object.FeeSplit) Then
		Form.Object.FeeSplit.Value=0.00
	End If
	If IsNull(Form.Object.FeeSplit2) Then
		Form.Object.FeeSplit2.Value=0.00
	End If
	If IsNull(Form.Object.FeeSplit3) Then
		Form.Object.FeeSplit3.Value=0.00
	End If
	If IsNull(Form.Object.SplitAmount) Then
		Form.Object.SplitAmount.Value=0.00
	End If
	If IsNull(Form.Object.SplitAmount2) Then
		Form.Object.SplitAmount2.Value=0.00
	End If
	If IsNull(Form.Object.SplitAmount3) Then
		Form.Object.SplitAmount3.Value=0.00
	End If
End Sub

Sub cmdTotal_Click()
	Call DefineValue()
	BaseRate = Form.Object.AssignmentRate2.Value
	If Not(Form.Object.AssignmentRate2.Value<1) Then
		If Not IsNull(Form.Object.AssignmentRate2) And Form.Object.Fees_Provider.Value=0.00 Then		
			Provider = (Form.Object.Fees_ProviderAmount * 100)/BaseRate
			Form.Object.Fees_Provider.Value=Round(Provider,2)
			Provider = Form.Object.Fees_ProviderAmount
		Else
			Provider = (Form.Object.Fees_Provider * BaseRate)/100
			Form.Object.Fees_ProviderAmount.Value=Round(Provider,2)
		End If
		
		If Not IsNull(Form.Object.AssignmentRate2) And Form.Object.Fees_OnCost.Value=0.00 Then		
			OnCost = (Form.Object.Fees_OnCostAmount * 100)/BaseRate
			Form.Object.Fees_OnCost.Value=Round(OnCost,2)
			OnCost = Form.Object.Fees_OnCostAmount
		Else
			OnCost = (Form.Object.Fees_OnCost * BaseRate)/100
			Form.Object.Fees_OnCostAmount.Value=Round(OnCost,2)
		End If
		
		If Not IsNull(Form.Object.AssignmentRate2) And Form.Object.Fees_Admin.Value=0.00 Then
			Admin = (Form.Object.Fees_AdminAmount * 100)/( Provider  + BaseRate + OnCost)	
			Form.Object.Fees_Admin.Value=Round(Admin,2)	
			Admin = Form.Object.Fees_AdminAmount.Value
		Else
			Admin = (( Provider  + BaseRate + OnCost) * Form.Object.Fees_Admin)/100	
			Form.Object.Fees_AdminAmount.Value=Round(Admin,2)
		End If
			
		If Not(Form.Object.Fees_ProviderAmount.Value<1) Then
			Fees_ProviderAmount = Form.Object.Fees_ProviderAmount
			If Not IsNull(Form.Object.AssignmentRate2) And Form.Object.FeeSplit.Value=0.00 Then
				FeeSplit1 = (Form.Object.SplitAmount * 100)/Fees_ProviderAmount
				Form.Object.FeeSplit.Value=Round(FeeSplit1,2)
				FeeSplit1 = Form.Object.SplitAmount
			Else
				FeeSplit1 = (Form.Object.FeeSplit * Fees_ProviderAmount)/100
				Form.Object.SplitAmount.Value = Round(FeeSplit1,2)
			End If
			If Not IsNull(Form.Object.AssignmentRate2) And Form.Object.FeeSplit2.Value=0.00 Then
				FeeSplit2 = (Form.Object.SplitAmount2 * 100)/Fees_ProviderAmount
				Form.Object.FeeSplit2.Value=Round(FeeSplit2,2)
				FeeSplit2 = Form.Object.SplitAmount2
			Else
				FeeSplit2 = (Form.Object.FeeSplit2 * Fees_ProviderAmount)/100
				Form.Object.SplitAmount2.Value = Round(FeeSplit2,2)
			End If
			If Not IsNull(Form.Object.AssignmentRate2) And Form.Object.FeeSplit3.Value=0.00 Then
				FeeSplit3 = (Form.Object.SplitAmount3 * 100)/Fees_ProviderAmount
				Form.Object.FeeSplit3.Value=Round(FeeSplit3,2)
				FeeSplit3 = Form.Object.SplitAmount3
			Else
				FeeSplit3 = (Form.Object.FeeSplit3 * Fees_ProviderAmount)/100
				Form.Object.SplitAmount3.Value = Round(FeeSplit3,2)
			End If
		End if
		Form.Object.TotalFees = Round(BaseRate + Provider + OnCost +  Admin,2)
		Form.Object.TotalAmount=Round(Provider + OnCost + Admin,2)
		Form.Object.NetTotal = Round((Provider + OnCost) - (FeeSplit1+FeeSplit2+FeeSplit3),2)
	End If
End Sub
%>
