form "CandidateImage" for "Candidate" 240,176
{
	"cntlPhoto" control "Elk9utl.Picture.1" 0,0,240(100),176(100) ENU "cntlPhoto"
	{
	}
	"txtNoPhoto" text tag ENU "No photo loaded" 82(50),82(50),60(50),7(50) autosize
	OnInit "Form_Init"
	Script VBScript
<%
Sub FillDocument()
	' if the document is embedded, and not dirty then load to a temp file then 
	' display the image on the screen
'	On Error Resume Next
	If Not me.parent.Object.Photo.IsNull Then
		If Not IsNull(me.parent.Object.Photo.Content) Then
			If me.parent.Object.Photo.mimetype="image/jpeg" Or	me.parent.Object.Photo.mimetype="image/bmp" Or me.parent.Object.Photo.mimetype="image/png" Then
				form.cntlPhoto.control.LoadFromFile me.parent.Object.Photo.extract()
				form.txtNoPhoto.Visible=False
				form.cntlPhoto.Visible=True
			Else
			Form.txtNoPhoto.Text="Photo format not supported"
			form.txtNoPhoto.Visible=True
			form.cntlPhoto.Visible=False
			End If
		Else
		Form.txtNoPhoto.Text="No photo loaded"
			form.txtNoPhoto.Visible=True
			form.cntlPhoto.Visible=False
		End If 
  	Else
  			Form.txtNoPhoto.Text="No photo loaded"
			form.txtNoPhoto.Visible=True
			form.cntlPhoto.Visible=False
	End If	
'	On Error Goto 0
End Sub

Sub Form_Init()
	FillDocument
	form.cntlPhoto.Enabled=False
End Sub
%>
}

