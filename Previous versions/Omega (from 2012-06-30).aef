menu "stdPerson" for "stdPerson"
{
	item "CandidateMorph" "On_CandidateMorph"ENU "Morph into Candidate"
	Script VBScript
<%
Sub On_CandidateMorph(Control)   
	Dim objCandidate, objTransaction, objMember, _
		objFolder, objClassInst, strJoinMember, intConfirm
	
	
	intConfirm=MsgBox("Turn this Person into a Candidate and reconnect all subfolders?", _
		vbYesNo, System.Title)
	If intConfirm=6 Then
		Set objCandidate=system.folders("Candidate").CreateNewInstance()
		Set objTransaction=system.BeginTransaction
			
		For Each objMember in This.Members 
			If objMember.Definition.MemberType=2 Then
				objCandidate.Members(objMember.Definition.Name).copy objMember.value	
			Else
				objCandidate.Members(objMember.Definition.Name).value= objMember.value
			End If
		Next  
		
		For Each objFolder in This.Folders     
			strJoinMember=objFolder.JoinMember.Name
			For Each objClassInst in objFolder
				objClassInst.Members(strJoinMember)=objCandidate
				objClassInst.Save objTransaction
			Next
		Next 
		
		objCandidate.Save objTransaction
		If objTransaction.validate.status<>3 Then
			objTransaction.Commit
		End If
		
		Set objTransaction=Nothing			
		Control.OpenObject(objCandidate)   
		This.Delete
	End If
End Sub
%>
}

