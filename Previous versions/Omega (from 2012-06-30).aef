	"txtContactText" edit tag ENU "Contact Email Text" 157(50),189,154(100),118(100) multiline autohscroll autovscroll hscroll vscroll toptag
	"optLocationChoose" radioset tag ENU "Location Selection" 0,0,312(100),18 spaceevenly stretchtofit Orientation(Horizontal) Alignment(left,top)
	values
	{
		value "1"
		{
			ENU "Use Contact's Primary Department"
		}
		value "2"
		{
			ENU "Choose another Department"
		}
	}
		OnChange "optLocationChoose_Change"
	"DepartmentList" control "Elk9.ChooserList.1" 0,20,312(100),114 ENU "DepartmentList"
	Events
	{
		"Click" "DepartmentList_Click"
	}
	{
		"_cx" = "5477"
		"_cy" = "11986"
		"Enabled" = "-1"
		"HasSource" = "0"
		"HasSegments" = "0"
		"HasSearch" = "-1"
		"ListClass" = "Elk9.ListView.1"
		"BackColor" = "4294967295"
		"ForeColor" = "4294967295"
		"FolderSet" = ""
		"List" = "Elk9.ListView.1:_cx=5080;_cy=5080;ShowIcons=True;MaxItems=0;FormControl=;BackColor=2147483653;BorderStyle=1;Enabled=True;ForeColor=4294967295;ContextMenuEnabled=True;ShowCount=True"
	}
	"frmLocationDetails" frame tag ENU "Include these Details" 0,136,312(100),41
	OnInit "Form_Init"
	Script VBScript
<%
Sub Form_Init()  
	
	
    Form.txtCandidateText.Readonly=True
    Form.txtContactText.Readonly=True
    Form.AddressCheck.value=TRUE
    '--Set the Email Text of this object to NULL, this will be used
    '--to show if the user has chosen a department and generated
    '--any text to send
    Form.Object.CandidateEmailText=""
    Form.Object.ContactEmailText=""
    Form.optLocationChoose.Value=1
    Form.optLocationChoose_Change
End Sub

Sub DepartmentList_Click(SelectedItem, Context, IsFolder) 
	If not SelectedItem is Nothing Then
		UpdateConfirmationText()
	End If
End Sub

Sub AddressCheck_Change() 
	UpdateConfirmationText()
End Sub

Sub DepartmentNameCheck_Change()
	UpdateConfirmationText()
End Sub

Sub UpdateConfirmationText()

		Form.txtCandidateText.value=Form.Object.BuildConfirmationText(Form.DepartmentList.Control.ListControl.SelectedItem, form.Object.Candidate.Candidate.Name.FirstName,form.Object.Contact.Contact.Person.Name.Prompt,Form.AddressCheck.value, Form.DepartmentNameCheck, Form.LocationNoteCheck)
		Form.Object.CandidateEmailText=form.txtCandidateText.Value
		
		Form.txtContactText.value=Form.Object.BuildConfirmationText(Form.DepartmentList.Control.ListControl.SelectedItem, form.Object.Contact.Contact.Person.Name.FirstName,form.Object.Candidate.Candidate.Prompt,Form.AddressCheck.value, Form.DepartmentNameCheck, Form.LocationNoteCheck)
		form.Object.ContactEmailText=form.txtContactText.Value
		
End Sub

Sub LocationNoteCheck_Change() 
	UpdateConfirmationText()
End Sub

Sub optLocationChoose_Change()
	FillDepartmentList ()
End Sub

Sub FillDepartmentList ()
	Dim Filterr
	
	Form.Object.CandidateEmailText=""
    Form.Object.ContactEmailText=""
    
	Set oDepartments = CreateObject("Elk9.Folder")
	oDepartments.Clear
	oDepartments.folder=system.folders.Department
	 
	Filterr = ""
	Set oDepartmentsFilter = CreateObject("ActivElk.Filter")
	Select Case form.optLocationChoose.Value
		Case 1 oDepartmentsFilter.Parse "Key=""" & form.Object.Contact.Contact.PrimeDepartment.Department.Key &"""" ,System.Classes("Department"),Filterr	
	 
	 	Case 2 oDepartmentsFilter.Parse "Organisation.Name=""" & RTrim(Form.Object.Location) &"""" ,System.Classes("Department"),Filterr
	End Select
	
	If Filterr="" Then
	    oDepartments.Filter = oDepartmentsFilter
			Select Case oDepartments.Folder.Count(oDepartments.Filter.Format(oDepartments.Folder.Targetclass))
				Case 0
					Form.DepartmentList.Control.ChooserControl.Folder=system.Folders.Department	
				Case Else
					Form.DepartmentList.Control.ChooserControl.Elkfolder=oDepartments
			End Select
	Else

		Form.DepartmentList.Control.ChooserControl.Folder=system.Folders.Department
	End If
	Form.DepartmentList.Control.ChooserControl.HasSearch=True
    Form.DepartmentList.Control.ListControl.fill
    
    Set oDepartments = Nothing
End Sub
%>
}

