script VBScript
<%
function OptimizeSegments(Progress)
	OptimizeAll=False
	dim i : i=0
	if not (Progress is nothing) then _
		Progress.Initialize System.Segments.Count,System.Segments.Title
	dim Segment
	for each Segment in System.Segments
		if not (Progress is nothing) then
			if Progress.Cancelled then exit function
			i=i+1
			Progress.Update Segment.Title,i
		end if
		Segment.Optimize
	next
	OptimizeAll=True
end function
%>

