Sub Optimize
	Set TheFilter=CreateObject("ActivElk.Filter")
	If This.UserParams<>"" Then
		Dim UserDef : Set UserDef=CreateObject("MSXML.DOMDocument")
		If UserDef.loadXML(This.UserParams) Then
			Dim Params : Set Params=UserDef.selectSingleNode("params")
			If not Params is nothing Then
				For Each Node in Params.selectNodes("param")
					TheFilter.AddSymbol "UserVal" & Node.getAttribute("id"),0
				Next
			End If
		End If
	End If
	Dim sError : sError=""
	Dim TheClass : Set TheClass=This.ObjClass
	On Error Resume Next
	If TheFilter.Parse(This.Expression,TheClass,sError) Then TheClass.OptimizeFilter TheFilter
End Sub
