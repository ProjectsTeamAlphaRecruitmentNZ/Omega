sysclass "WF_PendingWork" from "WF_PendingWork"
{
	
	folder "WF_PendingWork"
	{
		title(Singular)
			DEU "Workflow Ausstehende Arbeit",
			ELL "Εκκρεμούσα Εργασία Ροή-Εργασία",
			ENU "Workflow Pending Work",
			FRA "En attente de travail Workflow",
			ITA "Attività Workflow in sospeso ",
			JPN "Workflow 未処理ジョブ ",
			ZHS "工作流程待定工作"
		gender DEU feminine
		properties
		{
			"FolderSets" 0
				properties
				{
					"User" 0
				}
			"Hidden" "true"
		}
		filter "WhenDue <= Now And (IsNull(WF_Process.Suspended) Or Not WF_Process.Suspended) And (IsNull(WF_Job.Suspended) Or Not WF_Job.Suspended)"
		sort [ENU] "WhenDue" asc
	}
	
	folder "WF_PendingWorkByUser"
	{
		title(Singular)
			DEU "Workflow Ausstehende Arbeit nach Benutzer",
			ELL "Εκκρεμούσα Εργασία Ροή-Εργασία από Χρήστη",
			ENU "Workflow Pending Work By User",
			FRA "En attente de travail Workflow par l'uitilisateur",
			ITA "Attività Workflow in sospeso da parte dell’utente ",
			JPN "ユーザーによる Workflow 未処理ジョブ",
			ZHS "工作流程待定工作（按用户）"
		gender DEU feminine
		properties
		{
			"FolderSets" 0
				properties
				{
					"User" 0
				}
			"Hidden" "true"
		}
		filter "(WhenDue <= Now)  and (AssignedUser = User.Object)"
		sort [ENU] "WhenDue" asc
	}
	
	folder "WF_PendingWorkHidden"
	{
		title(Singular)
			ENU "WF_PendingWorkHidden",
			JPN "WF_PendingWorkHidden"
		properties
		{
			"FolderSets" 0
				properties
				{
					"User" 0
				}
			"Hidden" "TRUE"
		}
		sort [ENU] 
			"Status" asc,
			"Priority" asc,
			"WhenDue" asc
	}
}

