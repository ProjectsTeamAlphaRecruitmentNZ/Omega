Function GetTargetClassTopFolder()
	' Returns the Default Top Level folder for the target class for this WF_Job
	strTopFolder = ""
	If Not this.WF_Process.Value.IsNull Then
		If Not IsNull(this.WF_Process.Value.StartWithInst_class.value) Then
			strTopFolder = ""
			On Error Resume Next
			Set oClassDef = System.Classes(this.WF_Process.Value.StartWithInst_class.value)
			If not oClassDef is nothing Then
				Set oFolders = System.Folders.FoldersForClass(oClassDef)
				If oFolders.Count > 0 Then
					strTopFolder = oFolders.Item(0).Name
				End If
			End If
			On Error Goto 0
		End If
	End If
	GetTargetClassTopFolder = strTopFolder
End Function

Function GetTargetClass()

	' Return the target class for this job based on the selected process
	
	GetTargetClass = ""
	On Error Resume Next
	GetTargetClass = this.WF_Process.Value.StartWithInst_class.value
	On Error goto 0
	
End Function

Sub WF_DeleteJobMember(tobjobj, tobjAETrans)

	Dim objJobFolder
	Dim objJobEntry

	Set objJobFolder = This.Folders("WF_PendingWork").Scan("objkey='" & Tobjobj.Key & "'")
	For Each objJobEntry in objJobFolder
		If objJobEntry.objKey = Tobjobj.Key Then
			objJobEntry.Delete tobjAETrans
		End If
	Next

	Set objJobEntry = Nothing
	Set objJobFolder = Nothing
		
End Sub

Sub WF_AddFromFolder(tobjFolder, tblnAlreadyPending, tblnNewToJob, tbInitializeOnly)

	' Load from a Passed Folder

	Dim objAETrans
	Dim objFolderScan

	Set objAETrans = System.BeginTransaction
	objAETrans.AutoRevert = True

	Set objFolderScan = tobjFolder.Scan()
	For Each objObj In objFolderScan

		blnAddToJob = True

		' Check restriction on already pending items

		If blnAddToJob Then
			If tblnAlreadyPending Then

				' Check if the item already exists in the job

				If This.Folders("WF_PendingWork").Exists("objkey='" & objobj.Key & "' and Status='P'") Then
					blnAddToJob = False
				End If
			End If
		End If	

		' Check restriction on items that have ever been in the job

		If blnAddToJob Then
			If tblnNewToJob Then

				' Check if the item already exists in the job

				If This.Folders("WF_PendingWork").Exists("objkey='" & objobj.Key & "'") Then
					blnAddToJob = False
				End If
			End If
		End If

		If blnAddToJob Then
			System.WF_StartWithInstance WF_Process.Value, this, objobj, objAETrans, Nothing, tbInitializeOnly
		End If
	Next

	objAETrans.Commit

	Set objAETrans = Nothing
	Set objObj = Nothing
	Set objFolderScan = Nothing

End Sub

Sub WF_DeleteFromFolder(tobjFolder)

	' Remove from the job all items in the folder

	Dim objAETrans
	Dim objFolderScan
	Dim objObj
	
	Set objAETrans = System.BeginTransaction
	objAETrans.AutoRevert = True

	Set objFolderScan = tobjFolder.Scan()
	For Each objObj In objFolderScan
		WF_DeleteJobMember objObj, objAETrans
	Next
      
	objAETrans.Commit
 
	Set objAETrans = Nothing
	Set objObj = Nothing
	Set objFolderScan = Nothing
End Sub

Sub WF_AddFromBucket(tstrBucketID, tblnSelectedOnly, tblnAlreadyPending, tblnNewToJob, tbInitializeOnly)

	' Add from in a Bucket with the passed ID into this job

	Dim objAETrans
	Dim objBucketInstance
	Dim strBucketClass
	Dim objBucket
	Dim objBucketMember

	Set objAETrans = System.BeginTransaction
	objAETrans.AutoRevert = True

	If Len(Trim(tstrBucketID))>0 Then
		Set objBucketInstance = System.Classes.Bucket.GetInstance(tstrBucketID)
		If Not objBucketInstance Is Nothing Then
			strBucketClass = objBucketInstance.ClassName
			Set objBucket = System.Classes(strBucketClass).OpenBucket(tstrBucketID)
			If Not objBucket Is Nothing Then
				For Each objBucketMember in objBucket.Members
					blnAddToJob = False

					' Check if selected items only restriction and the item is selected 

					If tblnSelectedOnly Then
						blnAddToJob = objBucketMember.Selected
					Else
						blnAddToJob = True
					End If

					' Check restriction on already pending items

					If blnAddToJob Then
						If tblnAlreadyPending Then

							' Check if the item already exists in the job

							If This.Folders("WF_PendingWork").Exists("objkey='" & objBucketMember.Instance.Key & "' and Status='P'") Then
								blnAddToJob = False
							End If
						End If
					End If	

					' Check restriction on items that have ever been in the job

					If blnAddToJob Then
						If tblnNewToJob Then

							' Check if the item already exists in the job

							If This.Folders("WF_PendingWork").Exists("objkey='" & objBucketMember.Instance.Key & "'") Then
								blnAddToJob = False
							End If
						End If
					End If	

					If blnAddToJob Then
						System.WF_StartWithInstance WF_Process.Value, this, objBucketMember.Instance, objAETrans, Nothing, tbInitializeOnly
					End If
				Next
			End If
		End If
	End If	

	objAETrans.Commit

	Set objAETrans = Nothing
	Set objObj = Nothing
	Set objFolderScan = Nothing
End Sub

Sub WF_DeleteFromBucket(tstrBucketID, tblnSelectedOnly)

	' Delete items in a Bucket with the passed ID from this job

	Dim objAETrans
	Dim objBucketInstance
	Dim strBucketClass
	Dim objBucket
	Dim objBucketMember

	Set objAETrans = System.BeginTransaction
	objAETrans.AutoRevert = True

	If Len(Trim(tstrBucketID))>0 Then
		Set objBucketInstance = System.Classes.Bucket.GetInstance(tstrBucketID)
		If Not objBucketInstance Is Nothing Then
			strBucketClass = objBucketInstance.ClassName
			Set objBucket = System.Classes(strBucketClass).OpenBucket(tstrBucketID)
			If Not objBucket Is Nothing Then
				For Each objBucketMember in objBucket.Members
					blnDeleteFromJob = False

					' Check if selected items only restriction and the item is selected 

					If tblnSelectedOnly Then
						blnDeleteFromJob = objBucketMember.Selected
					Else
						blnDeleteFromJob = True
					End If

					If blnDeleteFromJob Then
						WF_DeleteJobMember objBucketMember.Instance, objAETrans
					End If
				Next
			End If
		End If
	End If	

	objAETrans.Commit

	Set objAETrans = Nothing
	Set objObj = Nothing
	Set objFolderScan = Nothing

End Sub

Sub PreSave(Transaction)
End Sub

Sub AddInitItems(tobjAETrans)
	' If WF_Process has been set, and property "InitItems" contains instances that
	' match the StartWithInstance_Class of WF_Process, then add those instances
	' to the job

	Set objWF_Process = this.WF_Process.Value
	Set Items = nothing
	if this.Properties.Exists("InitItems") Then
		Set Items = this.Properties("InitItems").Value
	End if

	If (Not objWF_Process.IsNull) and not (Items is nothing) Then
		strClass = objWF_Process.StartWithInst_class.value
		If (not IsNull(strClass)) and (Len(strClass) > 0) Then
			strClass = LCase(strClass)
			If this.Started Then
				bInitializeOnly = false
			Else
				bInitializeOnly = true
			End If
		
			' Add items.
			Dim Item
			For Each Item in Items
				' Only add items that are of the right class
				If (not (Item is Nothing)) and (Lcase(TypeName(Item)) = "iaeclassinst") and Item.IsKindOf(strClass) Then
					System.WF_StartWithInstance objWF_Process, this, Item, tobjAETrans, Nothing, bInitializeOnly
				End If
			Next
			this.Properties.Delete("InitItems")
		End If
	End If
End Sub
%>
}

