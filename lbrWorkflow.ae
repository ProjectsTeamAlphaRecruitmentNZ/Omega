<%
Function DoIt(hwnd, actionType)

	' Check that the instance is still pending before doing any work

	' Hang on to the current step and status so we can check after reload
	
	strStep = this.Step.Value
	
	' Force a reload of the instance
	
	this.Reload(0)
	
	' Check that the status and Step are the same and set the return value

	strStatus = this.Status.Value	
	If (strStatus = "P") and (this.Step.Value = strStep) Then
		' Set status to Processing
		this.Status.Value = "I"	  
		this.Save
		DoIt = 0
	Else
		DoIt = -1
		Exit Function
	End If

	If DoIt = 0 Then
		Set obj = CreateObject("ActivElk.Workflow.ClientEngine")
		DoIt = obj.DoIt(hwnd, this, (actionType))
	End If
		
End Function

Sub PreSave(Transaction)

	If This.Props.IsDirty Then
	    ' Get the Instance details from the Props member so Call Queue
	    ' management processes can do filters
	    
	    Dim objAEProperties
	    Set objAEProperties = CreateObject("ActivElk.WorkFlowProperties")
	    objAEProperties.Init System
		On Error Resume Next
	    objAEProperties.BinaryProps = This.Props.Value
		On Error Goto 0
		
		' Default the description to the prompt of the target instance
	
		Dim objTargetInstance
	    Set objTargetInstance = Nothing
	    If objAEProperties.Exists("Object") Then
	    	Set objTargetInstance = objAEProperties.Item("Object")
	    End If
		
		If Not objTargetInstance Is Nothing Then
			this.Descr = objTargetInstance.Prompt("WF_Prompt")
		End If   
		
		Set objTargetInstance = Nothing
		Set objAEProperties = Nothing
	End If
End Sub

Sub StartWithInstanceInit(tobjProcess, tobjJob, tobjObj, tobjAETrans, tobjParams, bInitializeOnly)
	' By overriding this function, derived classes can perform custom initialization 
	' while WF_StartWithInstance is being executed
End Sub

' attempt to lock the record. if successful, returns True
' do not forget to call Unlock when you are done with the record
Function Lock()
	If Status.Value <> "P" Then
		Lock = False
		Exit Function
	End If
	
	On Error Resume Next
	Dim objLock ' *-- 	Dim objLock as WF_PendingWorkLock
	Set objLock = Nothing
	
	'' this can fail at the app server level if an instance already exists with that key
	Set objLock = System.Folders.WF_PendingWorkLock.CreateNewInstance(System.Classes.WF_PendingWorkLock, 0, This.Key)
	On Error Goto 0
	
	If objLock Is Nothing Then
		Lock = False
		Exit Function
	End If
	
	Err.Clear
	On Error Goto 0
	' this can fail at the DB level if we get a primary key constraint
	' in which case the Locked check below will catch it
	objLock.Save
	On Error Resume Next
	
	If Err.Number <> 0 Then
		Lock = False
		Exit Function
	End If
	
	' reload and check if someone already completed processing the
	' pending work before we managed to get the lock
	This.Reload(0)
	If Status.Value <> "P" Then
		Lock = False
		Unlock
	End If	

	' hey we locked it	
	Lock = True
End Function

' unlock the instance. If you forget to unlock the instance it will remain locked until the
' end of time (or at least until the Workflow Server unlocks it)
Sub Unlock()
	Dim objLock ' *-- 	Dim objLock as WF_PendingWorkLock
	Set objLock = System.Classes.WF_PendingWorkLock.GetInstance(This.Key)
	
	If objLock.IsNull Then
		Exit Sub
	End If
	
	objLock.Delete		
End Sub
		
' check if this instance is locked
Function Locked()
	Dim objLock ' *-- 	Dim objLock as WF_PendingWorkLock
	Set objLock = System.Classes.WF_PendingWorkLock.GetInstance(This.Key)
	
	If objLock.IsNull Then
		Locked = False
	Else
		Locked = True
	End If	
End Function

Sub WF_Process_OnChange()
	This.CheckServerGroup
End Sub

Sub OnNew(hWnd)
	DoDefault hWnd
	
	This.CheckServerGroup
End Sub

Sub CheckServerGroup
	If Not WF_Process.IsNull Then
		WF_ServerGroup.Value = WF_Process.WF_ServerGroup.Value
	End If
End Sub

Sub OnValidate(Validation) ' *-- Sub OnValidate(Validation As IAeValidate)
	If (Not System.User.Object.IsAdmin) And Defn.IsDirty Then
		If InvalidProcessDefinition() Then
			Validation.Add System.LoadString("wf_InvalidProcessDefinition"), 3, This, Defn
		End If
	End If
End Sub

' security check for valid process definition - the definition must be copied from the workflow process
Function InvalidProcessDefinition()
	If Defn <> WF_Process.Defn Then
		InvalidProcessDefinition = True
		
		Dim objXML
		Set objXML = CreateObject("MSXML.DOMDocument")
		
		If Not objXML.loadXML(Defn) Then
			Exit Function
		End If
		
		Dim processNode
		Set processNode = objXML.DocumentElement.selectSingleNode("Process")
		If processNode Is Nothing Then
			Exit Function
		End If
		
		objXML.DocumentElement.removeChild(processNode)
		If objXML.XML = WF_Process.Defn Then
			InvalidProcessDefinition = False
		End If
	End If
End Function
