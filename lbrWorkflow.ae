Sub PreSave(Transaction)
	' lbrWorkflow
	' Check if the Process Definition has an StartWithInstance event
	bHasStartWithInstEvent = false
	
	' Get the class type for the job from the "AEWorkflow.StartWithInstance.Event"
	Set objXMLDom = CreateObject("MSXML.DomDocument")
	objXMLDom.Async = True
	If objXMLDom.LoadXML(this.defn.value) Then
		' Process definition loaded OK so get the Document Element
		Set objXMLDomElement = objXMLDom.DocumentElement
			
		' Then find all entryevents/event nodes
		Set objXMLEventNodes = objXMLDomElement.selectNodes("entryevents/event[@progid=""AeWorkFlow.StartWithInstance.Event""]")

		' Look for the aeworkflow.startwithinstance.event node
		For intLoop = 0 To (objXMLEventNodes.Length-1)
			Set objXMLEventNode = objXMLEventNodes.Item(intLoop)
			If objXMLEventNode.attributes.getNamedItem("progid").text = "AeWorkFlow.StartWithInstance.Event" Then
				' Check that the class is valid for this system
				strClass = ""
				strFirstStep = ""
				On Error Resume Next
				strClass = Lcase(objXMLEventNode.attributes.getNamedItem("class").text)
				strFirstStep = objXMLEventNode.attributes.getNamedItem("nextid").text
				Set objClassDef = Nothing
				Set objClassDef = System.Classes(strClass)
				On Error goto 0
				
				If Lcase(Typename(objClassDef)) = "iaeclassdef" Then
					this.StartWithInst_class = strClass
					this.StartWithInst_firststep = strFirstStep
					bHasStartWithInstEvent = true
					Exit For
				End If
			End If							
		Next
	End If

	If not bHasStartWithInstEvent Then
		this.StartWithInst_class = null
		this.StartWithInst_firststep = null
	End If
End Sub

Sub OnValidate(Validation)	 ' *-- Sub OnValidate(Validation As IAeValidate)	
	' Confirm that the WF_ProcID is entered is unique 	
	If Not IsNull(This.WF_ProcID.Value) Then
		If System.Folders("WF_Processes").Count("UCase(WF_ProcID)='" & UCase(This.WF_ProcID.Value) & "' And Key <> '" & This.Key & "'") > 0 Then
			strMessage = System.LoadString("WF_NoProcess")
			strMessage = System.FormatString(strMessage,Array(This.WF_ProcID.Value))
			Validation.Add strMessage, 3, This, This.WF_ProcID		
		End If	
	End If
End Sub
%>
}

