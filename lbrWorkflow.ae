Function WF_LogEntry(tstrProcess, tintType)

	' Write a WF Log Entry and return the instance to the caller so they
	' can write any details required

	Set WF_LogEntry = Nothing
	
	Set WF_LogEntry = System.Folders("LG_SystemEvent").CreateNewInstance(System.Classes("WF_LogEvent"))
	If Not WF_LogEntry Is Nothing Then
		With WF_LogEntry
			.Process = tstrProcess
			.WF_LogEventType = tintType
		End With	
	End If	

End Function

Function WF_StartWithInstance(tobjProcess, tobjJob, tobjObj, tobjAETrans, tobjParams, bInitializeOnly)

	' Create a new WF_PendingWork instance to run against a WF_Process whose entry event
	' is a StartWithInstance event
	'
	' tobjProcess 	  - the WF_Process to run against. Must have a StartWithInstance entry event
	' tobjJob		  - a WF_Job instance to which the newly-created WF_PendingWork
	'				    is to be joined. May be Nothing
	' tobjObj		  - the class instance to be used in the StartWithInstance event
	' tobjAETrans     - the transaction to be used to save the new instance. May be Nothing
	' tobjParams	 - additional parameters to be added as properties to the WF_PendingWork
	'				   instance. This must be an object of type "Scripting.Dictionary" or Nothing
	' bInitializeOnly - if true, then the status newly-created WF_PendingWork is set
	'				    to Initialized, otherwise it is set to Pending
	'
	' The return value is the new WF_PendingWork instance.
	
	Set WF_StartWithInstance = Nothing                             
	
	If tobjProcess is Nothing Then
		Exit Function
	End If
	If tobjObj is Nothing Then
		Exit Function
	End If

	' Check that the object is of the right class
	strClass = tobjProcess.StartWithInst_class.value
	If (IsNull(strClass) or (Len(strClass) = 0)) Then
		Exit Function
	End If
	If (Not tobjObj.IsKindOf(strClass)) Then
		Exit Function
	End If

	' Determine class of new PendingWork
	' Check if the Process Definition has an StartWithInstance event
	strPendingWorkClass = ""
	
	' Get the class type for the job from the "AEWorkflow.StartWithInstance.Event"
	Set objXMLDom = CreateObject("MSXML.DomDocument")
	objXMLDom.Async = True
	If objXMLDom.LoadXML(tobjProcess.defn.value) Then
		' Process definition loaded OK so get the Document Element
		Set objXMLDomElement = objXMLDom.DocumentElement
			
		' Then find all entryevents/event nodes
		Set objXMLEventNodes = objXMLDomElement.selectNodes("entryevents/event[@progid=""AeWorkFlow.StartWithInstance.Event""]")

		' Look for the aeworkflow.startwithinstance.event node
		For intLoop = 0 To (objXMLEventNodes.Length-1)
			Set objXMLEventNode = objXMLEventNodes.Item(intLoop)
			If objXMLEventNode.attributes.getNamedItem("progid").text = "AeWorkFlow.StartWithInstance.Event" Then
				strPendingWorkClass = ""
				On Error Resume Next
				strPendingWorkClass = Lcase(objXMLEventNode.attributes.getNamedItem("PendingWorkClass").text)
				On Error goto 0
				Exit For
			End If							
		Next
	End If
	If Len(strPendingWorkClass) = 0 Then
		strPendingWorkClass = "WF_PendingWork"
	End If
	
	On Error Resume Next
	Set objPendingWorkClassDef = Nothing
	Set objPendingWorkClassDef = System.Classes(strPendingWorkClass)
	On Error goto 0
	If Not Lcase(Typename(objPendingWorkClassDef)) = "iaeclassdef" Then
		Set objPendingWorkClassDef = System.Classes("WF_PendingWork")
	End If
	                             	
	' New PendingWork
	Dim objNewPendingWork
	Set objNewPendingWork = Nothing
                                 
	If not tobjJob is Nothing Then
		Set objNewPendingWork = tobjJob.Folders("WF_PendingWork").CreateNewInstance(objPendingWorkClassDef)
	Else
		Set objNewPendingWork = System.Folders("WF_PendingWork").CreateNewInstance(objPendingWorkClassDef)
	End If
		
	' Details of the instance being added
	objNewPendingWork.objClass = tobjObj.Class.Name
	objNewPendingWork.objKey = tobjObj.Key
	objNewPendingWork.Descr = tobjObj.Prompt("wf_prompt")
		
	Dim objXML
	Set objXML = CreateObject("MSXML.DOMDocument")
	
	If Not objXML.loadXML(tobjProcess.Defn) Then
		Err.Raise 1, "", "Invalid workflow process definition"
	End If
	
	Dim processNode
	Set processNode = objXML.CreateElement("Process")
	processNode.setAttribute "title", tobjProcess.title
	objXML.documentElement.appendChild(processNode)
	
	' Details of the process being followed
	objNewPendingWork.Defn = objXML.xml
	
	' Create the Props value
	Set objAEWorkFlowProperties = CreateObject("ActivElk.WorkFlowProperties")
	objAEWorkFlowProperties.Init System
	objAEWorkFlowProperties.Add "Object", tobjobj
	' Set to aeProcess lifetime (value 1)
	objAEWorkFlowProperties.SetLifetime "Object", 1
	
	objAEWorkFlowProperties.Add "WF_StartWithInstanceUserID", System.User.ID
	objAEWorkFlowProperties.SetLifeTime "WF_StartWithInstanceUserID", 1

	' Add any additional properties
	If not (tobjParams is nothing) then
		If (Lcase(typeName(tobjParams)) = "dictionary") Then
			For Each strKey in tobjParams.Keys
				objAEWorkFlowProperties.Add strKey, tobjParams.Item(strKey)
				' Set to aeProcess lifetime (value 1)
				objAEWorkFlowProperties.SetLifetime strKey, 1
			Next
		End If
	End If
	
	objNewPendingWork.Props = objAEWorkFlowProperties.BinaryProps

	objNewPendingWork.WF_Process.Value = tobjProcess

	' Perform custom initialization	
	objNewPendingWork.StartWithInstanceInit tobjProcess, tobjJob, tobjObj, tobjAETrans, tobjParams, bInitializeOnly
	
	'jwm - due to an "error" in CClientEngine::SetNextStep where Save is called w/o passing tobjAETrans
	'	if the following line has been executed prior to that happening then we will error with
	'	the "Object is already in an uncommited trans." error, which will mask the true error.
	'	allow the save post that call to handle things.
	'objNewPendingWork.Save tobjAETrans
	
	' Use the WorkflowClientEngine to set the first step
	Set objWorkFlowEngine = CreateObject("ActivElk.Workflow.ClientEngine")
	objWorkFlowEngine.SetNextStep objNewPendingWork,tobjProcess.StartWithInst_firstStep,tobjAETrans
	Set objWorkFlowEngine = Nothing
                                                      
	If bInitializeOnly Then
		objNewPendingWork.Status.Value = "N"                                                      
	Else
		objNewPendingWork.Status.Value = "P"                                                      
	End If
	objNewPendingWork.Save tobjAETrans

    ' Return the newly Workflow instance to the caller in case they want
    ' to add any extra stuff
	Set WF_StartWithInstance = objNewPendingWork
	
End Function

' Add specific parameters for tobjItem from tobjItemParams to tobjParams
Sub WF_AddItemParams(tobjItem, tobjItemParams, tobjParams)
	Dim strObjKey
	strObjKey = tobjItem.Key
	Dim objDict
	Set objDict = tobjItemParams.Item(strObjKey)
	If not (objDict is nothing) then
		If (Lcase(typeName(objDict)) = "dictionary") Then
			For Each strKey in objDict.Keys
				tobjParams.Add strKey, objDict.Item(strKey)
			Next
		End if
	End If
End Sub

' Remove specific parameters for tobjItem from tobjParams
Sub WF_RemoveItemParams(tobjItem, tobjItemParams, tobjParams)
	Dim strObjKey
	strObjKey = tobjItem.Key
	Dim objDict
	Set objDict = tobjItemParams.Item(strObjKey)
	If not (objDict is nothing) then
		If (Lcase(typeName(objDict)) = "dictionary") Then
			For Each strKey in objDict.Keys
				tobjParams.Remove strKey
			Next
		End If
	End If
End Sub

Function WF_CreateJob(strTitle, strDescr, tobjWF_Process, Items, tobjAETrans, tobjParams, tobjItemParams, bShowForm, tobjParentForm)

	' Create a new WF_Job instance to run against a specified WF_Process, and
	' populate it with the given items.

	' strTitle		 - the title for the new job.  It may be "".
	' strDescr		 - the description for the new job.  It may be "".
	' tobjWF_Process - the Workflow process to be run.  This Workflow process 
	'				   may only have one entry event, and it must be a 
	'				   StartWithInstance event.  This parameter may be null, 
	'				   in which case the WF_Process will need to be specified 
	'				   in the WF_Job form.
	' Items			 - an collection of instances to be passed to the StartWithInstance 
	'				   event of the WF_Process.  It can be either an IAeFolderScan
	'				   or an array of instances.  The class of the instances must 
	'				   match the class specified for the StartWithInstance event 
	'				   of the Workflow process.
	' tobjAETrans	 - a transaction object
	' tobjParams	 - additional parameters to be added as properties to each WF_PendingWork
	'				   instance.  This must be an object of type "Scripting.Dictionary" or Nothing
	' tobjItemParams - more additional parameters to be added as properties to WF_PendingWork
	'				   instance.  Unlike tobjParams, however, which are added to each WF_PendingWork,
	'				   these are added, each to its corresponding instance.   This must be an object 
	'				   of type "Scripting.Dictionary" or Nothing.  Each key in the scripting dictionary
	'				   is a key of an IAeClassInst in the Items collection, and the corresponding value 
	'				   is a nested Scripting dictionary, containing the additional parameters to add as
	'				   properties for that particular instance.
	' bShowForm		 - indicates whether the form for newly-created WF_Job is to 
	'				   be displayed to allow further configuration
	' tobjParentForm - the form to be used as the parent when bShowForm is true

	' The return value is the new WF_Job instance.

	Set WF_CreateJob = Nothing                             

	' Check that tobjWF_Process has a StartWithInstance	event
	strClass = ""
	If not (tobjWF_Process is Nothing) Then
		strClass = tobjWF_Process.StartWithInst_class.value
		If IsNull(strClass) or (Len(strClass) = 0) Then
			Set tobjWF_Process = nothing
			strClass = ""
		End If
	End If

	' New WF_Job
	Dim objNewJob
	Set objNewJob = Nothing
	Set objNewJob = System.Folders("Workflow Jobs").CreateNewInstance(System.Classes("WF_Job"))

	objNewJob.Title = strTitle
	objNewJob.Descr = strDescr

	objNewJob.WF_Process = tobjWF_Process

	' Add items.  This can only be done if the WF_Process is not null and valid (has StartWithInstance event)
	Dim bInitItems
	bInitItems = true
	If (Len(strClass) > 0) Then
		strClass = LCase(strClass)

		' Are there any parameters specific to instances?
		bItemParams = false
		If not (tobjParams is nothing) Then
			If (Lcase(typeName(tobjItemParams)) = "dictionary") Then
				bItemParams = true
			End If
		End If
		
		Dim Item
		if (Lcase(typeName(Items)) = "iaefolderscan") Then
			For Each Item in Items
				' Only add items that are of the right class
				If (not (Item is Nothing)) and (Lcase(TypeName(Item)) = "iaeclassinst") and Item.IsKindOf(strClass) Then
					If bItemParams Then
						WF_AddItemParams Item, tobjItemParams, tobjParams
					End If
					System.WF_StartWithInstance tobjWF_Process, objNewJob, Item, tobjAETrans, tobjParams, bShowForm
					If bItemParams Then
						WF_RemoveItemParams Item, tobjItemParams, tobjParams
					End If
				End If
			Next
		Else
			For i = LBound(Items) To UBound(Items) 
				' Only add items that are of the right class
				set Item = Items(i)
				If (not (Item is Nothing)) and (Lcase(TypeName(Item)) = "iaeclassinst") and Item.IsKindOf(strClass) Then
					If bItemParams Then
						WF_AddItemParams Item, tobjItemParams, tobjParams
					End If
					System.WF_StartWithInstance tobjWF_Process, objNewJob, Item, tobjAETrans, tobjParams, bShowForm
					If bItemParams Then
						WF_RemoveItemParams Item, tobjItemParams, tobjParams
					End If
				End If
			Next
		End If
		bInitItems = false
	End If
	if bInitItems then
		objNewJob.Properties("InitItems") = Items
	End If
		
	objNewJob.Save tobjAETrans

	If bShowForm and (not (tobjParentForm is Nothing)) Then
		tobjParentForm.FireOpenFormEvent objNewJob
	End If

	Set WF_CreateJob = objNewJob
End Function
