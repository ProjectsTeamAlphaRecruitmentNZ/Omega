form "stdAddressState" for "stdBaseAddress" 277,78
{
	"Address" edit member "Address" 0,0,276(100),38(100) multiline autovscroll vscroll
	"City" edit member "City" 0,38(100),96(50),24(100) autohscroll toptag
		OnLostFocus "City_LostFocus"
	"State" combobox member "State" 100(50),38(100),100(100),24(100) toptag
	"PostCode" edit member "PostCode" 204(100),38(100),73(100),24(100) autohscroll choose toptag
	OnChoose "PostCode_Choose"
	"Country" combobox member "Country" 0,64(100),277(100),14(100) icon
		OnChange "Country_Change"
	OnInit "Form_Init"
	Script VBScript
<%
Sub City_LostFocus()

	On Error Resume Next
	blnCheckPostCode = False
	blnCheckPostCode = Form.Object.City.Definition.Properties("CheckPostCode")
	On Error Goto 0
	
	If Len(RTrim(This.Parent.Postcode.value))=0 And blnCheckPostCode Then

		' Locate the Post Codes for this Address

		Form.Object.LocatePostCodes False

	End If

End Sub

Sub Country_Change()
	If Not Form.Object.Country.IsNull Then
		If Form.Object.Country.HasStates Then
			sStateName=Form.Object.Country.StateName
			If sStateName="" Then sStateName=System.LoadString("stdStateName")
			Form.State.Tag=sStateName
		End If
	End if
End Sub

Sub Form_Init()
	If Not Form.Object.Country.IsNull Then
		If Form.Object.Country.HasStates Then
			sStateName=Form.Object.Country.StateName
			If sStateName="" Then sStateName=System.LoadString("stdStateName")
			If sStateName<>"" Then Form.State.Tag=sStateName
		End If
	End if
End Sub

Sub PostCode_Choose()
	Form.Object.LocatePostCodes True
End Sub
%>
}

