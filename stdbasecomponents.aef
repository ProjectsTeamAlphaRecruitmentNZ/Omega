	Dim objPostCode

	If Not objListView.Control.SelectedItemisFolder Then
		Set objPostCode = objListView.Control.SelectedItem
		If Not IsNullObject(objPostCode) Then
			With Form.Object		
				.PostCode = objPostCode.PostCode
