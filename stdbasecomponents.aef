		"Orientation" = "1"
	}
	OnInit "Form_Init"
	Script VBScript
<%
Sub Form_Init()
	FillDocument
End Sub

Sub FillDocument()
	' if the document is embedded, and not dirty then load to a temp file then 
	' display the image on the screen
'	On Error Resume Next
	If Not IsNull(Form.Object.Content) And Not Form.Object.Content.IsDirty Then
		This.stdDocumentPreview.Control.Navigate2 Form.Object.Extract()
	Else
		This.stdDocumentPreview.Control.Navigate2 "about:blank"
	End If   
'	On Error goto 0
End Sub

Sub stdDocumentToolbar_Clicked(btnID)    
	If btnID = 1 Then 
	
		With Form.oleCommonDialog.Control
			.Filter = "*.*"
			.FilterIndex = 0
			.ShowOpen
		End With
	
		' Check if the selected file is valid
		If Len(Trim(Form.oleCommonDialog.Control.FileName)) > 0 Then
			If Form.Object.Embed(Form.oleCommonDialog.Control.FileName) Then
				Set objFileSystem = CreateObject("Scripting.FileSystemObject")
				Set objFileObject = objFileSystem.GetFile(Form.oleCommonDialog.Control.FileName)
				Form.Object.Filesize = Int(objFileObject.Size/1024)
				Form.FireChangeEvent
			End If
		End If

	End If
	
	If btnID = 2 Then
		If Not IsNull(Form.Object.Content) Then
			Set objUtils = CreateObject("Elk9.Utils")
			strFileName = Form.Object.Extract()
			objUtils.ShellExecute strFileName 
			Set objUtils = Nothing
		Else
			MsgBox "No document has been loaded.",0,System.Title
		End If

	End If

End Sub
%>
}

form "stdWWWAddress" for "stdWWWAddress" 226,14
{
	"WWWAddress" edit member "WWWAddress" 0,0,226(100),14 autohscroll choose
	OnChoose "WWWAddress_OnChoose"
	Script VBScript
<%
Sub WWWAddress_OnChoose()
	' Launch the browser to display the URL
	'Bhoomi | 25-02-2015 | If Linked IN field is not having http:// then append the string and redirect to the page
	Dim wwwStr
	wwwStr = this.parent.wwwAddress.value
	If Len(RTrim(this.parent.wwwAddress.value)) > 0 Then
		If Not InStr(1, wwwStr, "http://", 1) = 1 Then
			this.parent.wwwAddress.value =  "http://" & wwwStr
			This.Parent.Object.Display			
		Else
			This.Parent.Object.Display
		End If
	End If
End Sub
%>
}

