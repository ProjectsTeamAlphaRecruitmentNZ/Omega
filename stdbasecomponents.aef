form "CompositeEdit" for "stdCompositeEdit" 226,14
{
	"txtField" edit tag ITA "" 0,0,226(100),14 autohscroll choose
		OnLostFocus "txtField_LostFocus"
	OnChoose "txtfield_OnChoose"
	OnInit "Form_Init"
	OnPreSave "Form_PreSave"
	Script VBScript
<%
Sub Form_Init()
	UpdateText
End Sub

Sub txtField_LostFocus()
	' When focus is lost parse into components
	If Form.Object.ParseComposite(Form.txtField.Value) Then UpdateText
End Sub

Sub txtfield_OnChoose()
	Form.Object.ParseComposite Form.txtField.Value
	If Form.Object.EditDetails(Wnd) Then 
		
		' Call the underlying class to allow any formatting required
		
		Form.Object.AfterEditDetails

		' Update the Value Displayed
		
		UpdateText
	End If
End Sub

Sub Form_PreSave()
	' When focus or prior to save then parse into components
	If Not Form.Object.ParseComposite(Form.txtField.Value) Then Form.Object.EditDetails Wnd
End Sub

Sub UpdateText
	Form.txtField.Value=Form.Object.BuildComposite
	Form.txtField.IsDirty=Form.Object.IsDirty
	If Form.txtField.IsDirty Then
		Form.FireChangeEvent
	End If
End Sub
%>
}

